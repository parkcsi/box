@charset "utf-8";
/* 접근성 */
.skip{z-index:4;position:absolute;left:50%;top:0;margin-left:-250px;width:500px;}
.skip>li{position:absolute;left:50%;margin-left:-250px;top:0;width:500px;height:auto;}
.skip>li>a{display:block;padding:0;font-size:0;text-align:center;background:#fff;color:#000;}
.skip>li>a:focus{padding:15px 0;font-size:16px;box-sizing: border-box;}
/* 공통 */
.hidden {display:none;position:absolute;font-size:0;width:0;height:0;line-height:0;background:none;}
.blind {text-indent:-2000px;overflow:hidden;}
.content_new #container.cont_login {padding-top: 0;}
.clear:after {content: '';display: block;clear: both;height: 0;font-size: 0;}
/* 로그인 */
.loginWrap {position:relative;max-width:360px;margin:79.26px auto 0;}
.loginWrap .logo_img {text-align: center;}
.loginWrap .tab_login {margin-top:39.74px;}
.loginWrap .tab_login li {float:left;width:50%;text-align: center;font-size:18px;}
.loginWrap .tab_login li a {display:block;padding:15px 0;box-sizing:border-box;}
.loginWrap a.btn_member_on, a.btn_nonmember_on {box-shadow: 0px 5px 10px rgba(0,0,0, .16);-moz-box-shadow:0px 5px 10px #00000029;border: 1px solid #000;background: #fff;}
.loginWrap a.btn_member_off, a.btn_nonmember_off {border: 1px solid #eee;color:#999;background: #f8f8f8;}
/* IE input 초기화 */
input::-ms-clear, input::-ms-reveal {display: none;}
.loginWrap .inp_login{position: relative;}
.loginWrap .box_login legend{display:none;}
.loginWrap .inp_login label.lab_comment{display:block;font-size:14px;margin-top:36px;margin-bottom:6px;color:#999;}
.loginWrap .inp_pw label.lab_comment {margin-top:26px;}

.loginWrap .form_login::-webkit-input-placeholder{color:#dbdbdb;font-size: 18px;font-family:'Noto Sans KR', sans-serif;}
.loginWrap .form_login::-moz-placeholder{color:#dbdbdb;font-size: 18px;font-family:'Noto Sans KR', sans-serif;}
.loginWrap .form_login:-moz-placeholder{color:#dbdbdb;font-size: 18px;font-family:'Noto Sans KR', sans-serif;}
.loginWrap .form_login:-ms-input-placeholder{color:#dbdbdb;font-size: 18px;font-family:'Noto Sans KR', sans-serif;}
.loginWrap .inp_login .form_login{width:100%;height:48px;background: #f8f8f8;border: 1px solid #eee;font-size:18px;padding-left:20px;box-sizing: border-box;}
.loginWrap input.form_login:focus{border: none;outline:1px solid #ff6a5b;box-sizing: border-box;}
.loginWrap .opt_login{margin-top:45px;text-align:right;}
.loginWrap .opt_login:after{content:'';display:block;clear:both;}
.loginWrap .opt_login .auto_save{float:right;}

.loginWrap .inp_chk{display:inline-block;overflow:hidden;position:relative;font-size:0;margin-left:17px;line-height:14px;vertical-align:middle;}
.loginWrap .inp_chk input[type="checkbox"]{position:absolute;top:1px;left:1px;width:0;height:0;-webkit-appearance:none;-moz-appearance: none;appearance: none;outline: 0;}
.loginWrap .inp_chk label,.inp_chk span{display:inline-block;position:relative;padding-left:24px;font-size:14px;color:#777;padding-top: 4px;box-sizing: border-box;cursor:pointer;}
.loginWrap .inp_chk label:before,.inp_chk span:before{content:'';display:block;position:absolute;top:1px;left:1px;width:20px;height:20px;background:url(../../images/biz/sub/btn_check_off.png) no-repeat;border-radius:4px;background-position: 0 0;background-size:100%;box-sizing: border-box;border:1px solid #dbdbdb;}
.loginWrap .inp_chk input[type="checkbox"]:checked + label:before,.inp_chk input[type="checkbox"]:checked + span:before{background:url(../../images/biz/sub/btn_check_on.png) no-repeat;background-size:100%;border:none;}
.loginWrap .btn_login{display:block;width:100%;margin-top:15px;font-size:22px;font-family: 'Noto Sans KR', sans-serif;}
.loginWrap .btn_login.off{color:#000;border: 1px solid #000;background: #fff;padding:8.5px 0;box-sizing: border-box;cursor: default;}
.loginWrap .btn_login.on{color:#fff;border:0;background: #000;padding:9.5px 0;cursor:pointer;}
.loginWrap p.txt_inquiry {color:#777;font-size:14px;margin-top:45px;}
/* 아이디찾기 등 */
.loginWrap .list_loginEtc {margin-top:15px;padding-bottom:40px;border-bottom:1px solid #eee;}
.loginWrap .list_loginEtc li {float:left;width:33.33333%;text-align: center;}
.loginWrap .list_loginEtc li a {display: block;font-size:14px;color: #777;padding:5px 0 10px;}
.loginWrap .list_loginEtc li a.btn_register {color: #000;}
.loginWrap .login_naver {margin-top:30px;text-align: center;}
.loginWrap .login_naver a {font-size:18px;}
.loginWrap .login_naver a img {width:32px;height:32px;margin-right:8px;}
/* 로그인 하단배너 */
.loginWrap .bnr_login {display:table;width:100%;height: 180px;background: #f4f2f5;margin-top:58px;margin-bottom:70px;}
.loginWrap .bnr_login a {display: table-cell;vertical-align: middle;width:100%;height:100%;text-align: center;font-size:20px;color:#dbdbdb;}
.loginWrap .bnr_login a img {max-width:100%;max-height:180px;}
.loginWrap .txt_center {font-size:11px;text-align: center;letter-spacing: -0.55px;line-height: 20px;color: #666;font-size:11px;}
.loginWrap span.bt_bar {display: block;width:100%;height:1px;background: #eee;margin:10px 0;}
.loginWrap p.copyright {font-size:11px;text-align: center;color:#999;}

.loginWrap .box_register {margin-top:40px;}
.loginWrap .box_register>p {letter-spacing: -0.28px;color: #777;font-size:14px;line-height: 20px;}
.loginWrap a.link_register {font-size:14px;color: #000;letter-spacing: -0.28px;}
.loginWrap a.link_register img {width:8px;height:14px;margin-left:2px;}
.loginWrap .search,.searchClear {position: absolute;right: 18px;top: 36px;width: 16px;height: 16px;cursor: pointer;}
