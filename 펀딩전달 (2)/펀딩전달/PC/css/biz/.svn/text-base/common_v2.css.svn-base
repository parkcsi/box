/* 팝업 */
.layer_popup{display:none; overflow:hidden; position:fixed; top:0; left:0; bottom:0; right:0; z-index:999; -webkit-box-sizing:border-box; box-sizing:border-box;}
.layer_popup .inner_layer{position:absolute; top:50%; left:0; right:0; width:932px; height:auto; z-index:99; box-sizing:border-box; margin:0 auto;}
.layer_popup .layer_close{position:absolute; top:-60px; right:0; width:58px; height:58px; border:1px solid #000; background:#FF6A5B url('../../images/biz/sub/btn_popup.png') 50% 50% no-repeat; text-indent:-9999px;}
