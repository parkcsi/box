.pagination_v2{text-align:center; margin-bottom:80px;}
.pagination_v2 a{display:inline-block; color:#000; vertical-align:top; width:48px; height:48px; padding-top:15px; border-radius:24px; font-family:font_gs; font-size:22px; text-align:center; box-sizing:border-box;}
