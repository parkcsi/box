/* header */
.top_banner{position:relative; min-width:1280px; font-family:font_ns, AppleGothic, sans-serif;}
.top_banner .inner{position:relative; width:950px; margin:0 auto;}
.top_banner .btn_close{position:absolute; top:50%; margin-top:-10px; width:20px; height:20px; right:70px; z-index:5; text-indent:-9999px; background:url('../../images/biz/common/icon_close_banner.png') 50% 50% no-repeat;}

#header{position:absolute;; left:0; right:0; width:100%; top:0; z-index:999; background-color:#fff;}
#header .util_menu{position:relative; width:1280px; height:30px; margin:0 auto; display:block;}
#header .util_menu ul{overflow:hidden; float:right;}
#header .util_menu ul li{float:left; line-height:30px;}
#header .util_menu ul li a{display:block;color:#777;}
#header .util_menu ul li a.login{display:none;}
#header .util_menu.login ul li a.login{display:block;}
#header .util_menu.login ul li a.logout{display:none;}
#header .util_menu ul li + li{margin-left:22px;}

#gnb{position:relative; top:0; width:100%; min-width:1280px; border-bottom:1px solid #dbdbdb; background-color:#fff;}
#gnb .header_wrap{position:relative; width:1280px; margin:0 auto; height:154px;}
#gnb .g_logo{position:absolute; left:0; top:59px;}
#gnb .g_logo a{display:block; width:200px; height:37px; text-indent:-9999px; background:url('../../images/biz/common/logo.png') no-repeat;}
#gnb .g_search{position:absolute; width:800px; top:53px; left:240px; overflow:hidden; padding-bottom:13px; border-bottom:5px solid #000;}
#gnb .g_search .form_select{float:left; width:132px; vertical-align:middle; overflow:hidden; position:relative;}
#gnb .g_search .form_select:before{content:''; position:absolute; right:0; top:12px; width:14px; height:8px; background:url('../../images/biz/common/icon_select_arr.png') 0 0 no-repeat;}
#gnb .g_search .form_select select{position:relative; width:100%; border:0; height:32px; line-height:32px; font-size:20px; color:#000; padding-right:14px; background:transparent; z-index:1;}
#gnb .g_search .form_select select{-webkit-appearance: none; -moz-appearance: none; appearance: none;}
#gnb .g_search .form_select select::-ms-expand {display:none;}
#gnb .g_search .form_search{position:relative; float:left; height:32px; width:360px; vertical-align:middle;}
#gnb .g_search .form_search input{border:0; height:32px; text-indent:10px; width:290px; color:#000; font-size:20px; background:transparent;}
#gnb .g_search .form_search input::placeholder{color:#999; font-size:20px; font-family:font_ns, sans-serif;}
#gnb .g_search .form_search input{vertical-align:middle;  font-family:font_ns, sans-serif; -webkit-appearance:none;-webkit-border-radius:0;}
#gnb .g_search .form_search .btn_search{position:absolute; width:70px; height:32px; right:0; top:0; background:url('../../images/biz/common/icon_search.png') 50% 50% no-repeat;}
#gnb .g_search .form_search .btn_search:after{content:''; position:absolute; right:0; top:0; width:1px; height:32px; background-color:#000;}
#gnb .g_search .keyword_wrap{position:relative; float:left; height:32px; width:308px; vertical-align:middle; overflow:hidden;}
#gnb .g_search .keyword_wrap a {display:block; height:32px;}
#gnb .g_search .keyword_wrap a > *{display:inline-block; line-height:32px;}
#gnb .g_search .keyword_wrap a em.font_t{padding:2px 16px 0; color:#FF6A5B; font-size:22px; font-weight:bold; vertical-align:top;}
#gnb .g_search .keyword_wrap a p{width:220px; font-size:20px; color:#000; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
#gnb .g_search .keyword_wrap a .num{position:absolute; min-width:10px; text-align:center; right:0; padding-top:6px; height:32px; font-size:11px; color:#FC3E19; vertical-align:top; box-sizing:border-box; font-family:font_gs, sans-serif;}
#gnb .g_search .keyword_wrap a .num:before{content:''; position:absolute; right:50%; margin-right:-5px; top:50%; margin-top:-10px; width:10px; height:6px;}
#gnb .g_search .keyword_wrap a .num.up:before{background:url('../../images/biz/common/icon_updown.png') no-repeat; background-size:20px 6px;}
#gnb .g_search .keyword_wrap a .num.down{color:#419BF9;}
#gnb .g_search .keyword_wrap a .num.down:before{background:url('../../images/biz/common/icon_updown.png') no-repeat; background-size:20px 6px; background-position:-10px 0;}
#gnb .g_search .keyword_wrap a .txt{vertical-align:top;}
#gnb .g_search .keyword_list{}
#gnb .g_menu{position:absolute; right:30px; top:60px;}
#gnb .g_menu ul li{display:inline-block;}
#gnb .g_menu ul li + li{margin-left:40px;}
#gnb .g_menu ul li a{display:block; position:relative;}
#gnb .g_menu ul li.icon_today em{display:block; position:absolute; top:-6px; left:22px; width:64px; padding:0 6px; font-size:12px; line-height:21px; text-align:center; color:#fff; background:#FF6A5B; border-radius:10px; border-bottom-left-radius:0 !important; box-sizing:border-box;}
#gnb .g_menu ul li.icon_cart em{display:block; position:absolute; top:-6px; left:34px; min-width:28px; padding:6px; height:21px; font-size:13px; letter-spacing:-0.05em; font-weight:bold; text-align:center; color:#fff; background:#FF6A5B; border-radius:10px; box-sizing:border-box;}
#gnb .g_menu ul li.icon_my{display:none;}

#gnb .gnb_wrap{border-top:1px solid #dbdbdb;}
#gnb .gnb_wrap .inner{width:1280px; margin:0 auto; overflow:hidden;}
#gnb .btn_category{position:relative; width:200px; height:78px; background:#3A4152; color:#fff;}
#gnb .btn_category:before{position:absolute; content:''; left:38px; top:50%; margin-top:-13px; width:30px; height:24px; background:url('../../images/biz/common/icon_menu.png') no-repeat; background-size:90px 24px;}
#gnb .btn_category a{display:block; color:#fff; width:100%; height:100%; font-size:20px; padding:28px 0 0 85px; box-sizing:border-box;}
#gnb .category_wrap{position:absolute; width:200px; background:#fff;}
#gnb .category_cont{display:none; position:relative; height:552px; background:#fff; border:1px solid #424a5d; box-sizing:border-box;}
#gnb .category_cont.gnb_open{display:block;}
#gnb .category_cont.gnb_open.on{width:1080px !important;}
#gnb .category_cont ul{position:relative; display:inline-block; height:100%; padding:16px 0; box-sizing:border-box;}
#gnb .category_cont ul li a{position:relative; display:block; padding:8px 27px; color:#000; font-size:16px; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
#gnb .category_cont ul li a:hover{color:#DA2128;}
#gnb .category_cont ul li.active > a{color:#DA2128;}
#gnb .category_cont ul li.active > a:after{position:absolute; content:''; width:7px; height:11px; margin-top:3px; margin-left:8px; background:url('../../images/biz/common/icon_category_arr.png') no-repeat; background-position:-7px 0;}
#gnb .category_cont ul li a.all:after{position:absolute; content:''; width:7px; height:11px; margin-top:3px; margin-left:8px; background:url('../../images/biz/common/icon_category_arr.png') no-repeat;}
#gnb .category_cont ul li.active > a.all:after{background-position:-7px 0;}
#gnb .category_cont ul li a.more{color:#5eaefe;}
#gnb .category_cont ul li a.more:after{position:absolute; content:''; width:7px; height:11px; margin-top:3px; margin-left:8px; background:url('../../images/biz/common/icon_category_arr.png') no-repeat; background-position:-7px 0; background-position:-14px 0}
#gnb .category_cont > ul{width:199px; box-sizing:border-box;}
#gnb .category_cont.on > ul{border-right:1px solid #424a5d;}
#gnb .category_cont > ul > li .dp2{display:none; position:absolute; left:199px; top:0; bottom:0; width:255px; border-right:1px solid #eee; box-sizing:border-box;}
#gnb .category_cont > ul > li.active .dp2{display:block;}
#gnb .category_cont > ul > li .dp2 ul{width:100%;}
#gnb .category_cont > ul > li .dp3{display:none; position:absolute; left:254px; top:0; bottom:0; width:255px;}
#gnb .category_cont > ul > li.active .dp2 li.active .dp3{display:block;}
#gnb .category_cont.on .banner{display:block;}
#gnb .category_cont .banner{display:none; float:right; overflow:hidden; width:280px; height:550px; background:#e6f3ff;}
#gnb .category_cont .banner a{display:block;}
#gnb .category_cont .banner img{width:100%; height:100%;}

#gnb .gnb_menu{padding-left:240px; display:inline-block; overflow:hidden; vertical-align:top;}
#gnb .gnb_menu li{float:left;}
#gnb .gnb_menu li + li{padding-left:43px;}
#gnb .gnb_menu li a{position:relative; display:block; font-size:28px; padding:24px 0 26px;}
#gnb .gnb_menu li a em{content:''; position:absolute; display:inline-block; margin-top:-5px; width:6px; height:6px; border-radius:50%; background:#DA2128;}

#gnb .sub_menu{position:relative;display:block; padding-left:32px; float:right; overflow:hidden; vertical-align:top;}
#gnb .sub_menu:before{position:absolute; content:''; background:#DBDBDB; top:50%; margin-top:-14px; left:0; width:1px; height:28px;}
#gnb .sub_menu li{float:left;}
#gnb .sub_menu li + li{margin-left:27px;}
#gnb .sub_menu li a{position:relative; display:block; color:#000; font-size:12px; padding:44px 0 22px;}
#gnb .sub_menu li:nth-child(1){background:url('../../images/biz/common/icon_taste.png') 50% 35% no-repeat;}
#gnb .sub_menu li:nth-child(2){background:url('../../images/biz/common/icon_life.png') 50% 40% no-repeat;}
#gnb .sub_menu li:nth-child(3){background:url('../../images/biz/common/icon_azit.png') 50% 35% no-repeat;}

#header.fixed .util_menu{display:none;}
#header.fixed #gnb .gnb_menu{display:none;}
#header.fixed #gnb .sub_menu{display:none;}
#header.fixed #gnb .category_wrap{top:0;}
#header.fixed #gnb .header_wrap{height:112px;}
#header.fixed #gnb .g_logo{position:absolute; left:125px; top:36px;}
#header.fixed #gnb .g_logo a{width:132px; height:38px; background:url('../../images/biz/common/logo_fixed.png') no-repeat; background-size:100%;}
#header.fixed #gnb .g_search .form_search{width:100%; height:38px;}
#header.fixed #gnb .g_search .form_search input{width:583px; height:38px;}
#header.fixed #gnb .g_search{top:38px; left:328px; width:640px;}
#header.fixed #gnb .g_search .form_select{display:none;}
#header.fixed #gnb .g_search .form_search .btn_search{width:57px; height:38px; background:url('../../images/biz/common/icon_search02.png') 50% 50% no-repeat;}
#header.fixed #gnb .g_search .form_search .btn_search:after{display:none;}
#header.fixed #gnb .g_search .keyword_wrap{display:none;}
#header.fixed #gnb .g_menu{right:20px; top:32px;}
#header.fixed #gnb .g_menu ul li.icon_my{display:inline-block;}

#header.fixed #gnb .gnb_wrap{border:0;}
#header.fixed #gnb .category_wrap{width:90px;}
#header.fixed #gnb .btn_category{width:90px; height:112px; border:1px solid #fff; box-sizing:border-box; background:transparent;}
#header.fixed #gnb .btn_category a{padding:0; text-indent:-9999px; background:transparent;}
#header.fixed #gnb .btn_category:before{left:30px; background-position:-30px 0;}
#header.fixed #gnb .btn_category.active{border:1px solid #000; border-bottom:1px solid #fff;}
#header.fixed #gnb .btn_category.active:before{background-position:-60px 0;}
#header.fixed #gnb .category_cont{width:200px; border:1px solid #424a5d !important;}

/* location */
.location_wrap{position:relative; height:30px;}
.location_wrap .btn_home{position:relative; width:24px; height:30px; float:left; margin-right:4px; text-indent:-9999px;}
.location_wrap .btn_home:before{position:absolute; content:''; display:inline-block; left:50%; margin-left:-7px; top:8px; width:14px; height:13px; background:url('../../images/biz/common/icon_location.png') 0 0 no-repeat;}
.location_nav_wrap{float:left; font-size:12px;position:relative;}
.location_nav_wrap + .location_nav_wrap{margin-left:24px;}
.location_nav_wrap + .location_nav_wrap:before{content:''; display:inline-block; position:absolute; left:-16px; top:50%; margin-top:-5px; width:6px; height:11px; background:url('../../images/biz/common/icon_location.png') -22px 0 no-repeat;}
.location_wrap .location_1{width:auto; color:#777; line-height:30px; border-top:1px solid #fff; padding-left:10px; padding-right:45px; display:inline-block; border-left:1px solid #fff; border-right:1px solid #fff; box-sizing:border-box;}
.location_wrap .location_1:after{content:''; display:inline-block; position:absolute; right:15px; top:50%; margin-top:-2px; width:5px; height:3px; background:url('../../images/biz/common/icon_location.png') -15px 0 no-repeat;}
.location_wrap .location_1.active{border-right:1px solid #dbdbdb; border-top:1px solid #dbdbdb; border-left:1px solid #dbdbdb;}
.location_wrap p.location_1:after{display:none;}
.location_wrap .location_nav{position:absolute; left:0; top:30px; display:none; padding:5px 0; z-index:5; background:#fff; border:1px solid #dbdbdb; border-top:0; width:100%; box-sizing:border-box;}
.location_wrap .location_nav li{}
.location_wrap .location_nav li a{color:#777; height:30px; line-height:30px; display:block; padding:0 10px; text-overflow:ellipsis; white-space:nowrap; overflow:hidden;}
.location_wrap .location_nav li a:hover{color:#DA2128;}

.location_wrap .location_1.on{color:#000;}
.location_wrap .location_1.active + .location_nav{display:inline-block;}

/* content */
.content_new #container{padding-top:264px; min-width:1280px;}
.content_new .content{min-height:600px;}
.content_new .content .inner950{width:950px; margin:0 auto;}

.content_new #container .content.category_wrap{width:100%;}

.content_new .location_wrap{width:950px; margin:0 auto; margin-top:16px;}
.content_new .content h2.sub_title{text-align:center; font-size:28px; color:#000; padding:40px 0 30px; font-weight:normal;}
.content_new .content .category_list{width:950px; margin:0 auto; padding:30px 20px; margin-bottom:80px; border:1px solid #eee; border-top:2px solid #808080; box-sizing:border-box;}
.content_new .content .category_list ul{overflow:hidden; margin-top:-16px;}
.content_new .content .category_list ul li{float:left; width:25%; margin-top:15px;}
.content_new .content .category_list ul li a{display:inline-block; max-width:100%; font-size:16px; color:#000; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
.content_new .content .category_list ul li a:hover{color:#DA2128;}
.content_new .content .category_list ul li a:focus{color:#DA2128;}
.content_new .content .category_list .type02{border-top:1px solid #eee; margin-top:30px; padding-top:20px;}
.content_new .content .category_list .type02 ul li{width:auto;}
.content_new .content .category_list .type02 ul li + li{margin-left:40px;}

.content_new .sub_tab{padding:30px 0 20px 0;}
.content_new .sub_tab ul{text-align:center;}
.content_new .sub_tab ul li{position:relative; padding-bottom:12px; display:inline-block; font-family:font_gs, sans-serif;}
.content_new .sub_tab ul li a{font-size:28px; color:#777;}
.content_new .sub_tab ul li.active a{color:#000; font-weight:bold;}
.content_new .sub_tab ul li.active::after{position:absolute; left:0; right:0; bottom:0; content:''; background-color:#000; width:100%; height:4px;}
.content_new .sub_tab ul li + li{margin-left:35px;}

.category_wrap .category_banner_wrap{position:relative;}
.category_wrap .category_banner{position:relative; max-width:1920px; margin:0 auto; height:500px; overflow:hidden;}
.category_wrap .category_banner .slick-list{height:500px; overflow:hidden;}
.category_wrap .category_banner .slick-slide{position:relative; width:950px;}
.category_wrap .category_banner .slide-item.slick-center a:after{background-color:rgba(0,0,0,0);}
.category_wrap .category_banner .slide-item a{display:block;}
.category_wrap .category_banner .slide-item a:after{position:absolute;content:''; display:block; top:0; bottom:0; right:0; left:0; z-index:10; background-color:rgba(0,0,0,0.5); -webkit-transition:background .5s ease-in-out; -moz-transition:background .5s ease-in-out; -ms-transition:background .5s ease-in-out; -o-transition:background .5s ease-in-out; transition:background .5s ease-in-out;}
.category_wrap .category_banner .type01 .thum{width:950px; height:500px;}
.category_wrap .category_banner .type02 .thum{width:950px; height:350px;}
.category_wrap .category_banner .thum img{width:100%; height:100%;}
.category_wrap .category_banner .desc{position: absolute;bottom:0;width:100%;background:#faf3f2; height:150px; box-sizing:border-box; padding:38px 20px 0 60px;opacity:0.9;}
.category_wrap .category_banner .desc .tit{font-size:40px;font-weight:bold; text-overflow:ellipsis; white-space:nowrap; overflow:hidden;}
.category_wrap .category_banner .desc .txt{font-size:18px; margin-top:20px; color:#000; text-overflow:ellipsis; white-space:nowrap; overflow:hidden;}
.category_wrap .category_banner_wrap .custom_paging_wrap{position:absolute; width:950px; bottom:0; left:50%; margin-left:-475px; height:0; z-index:2;}
.category_wrap .category_banner_wrap .custom_paging{position:absolute; font-weight:300; padding-top:14px;text-align:center; bottom:0; right:0; width:120px; height:44px; background:rgba(0,0,0,0.4); font-size:20px; color:#fff; font-family:font_gs, sans-serif; box-sizing:border-box;}
.category_wrap .category_banner_wrap .custom_paging strong{margin-right:10px;}
.category_wrap .category_banner_wrap .btn_plus{position:absolute; display:block; right:12px; bottom:0; width:21px; height:44px; background:url('../../images/biz/sub/icon_plus.png') center no-repeat;}
.category_wrap .category_banner_wrap .slick-arrow {z-index: 3;position: absolute;outline: 0;width: 11px;bottom: 100px;height: 19px;left: 50%;bottom: 13px;margin-left: 360px;background: url(../../images/biz/main/icon_arr03.png) no-repeat;font-size: 0;text-indent: -9999px;}
.category_wrap .category_banner_wrap .slick-next {position: absolute;margin-left: 455px;background-position: 100% 0;}
.category_wrap .category_banner_wrap .slick-prev {position: absolute;margin-left: 365px;}
.category_wrap .tab{width:950px; height:60px; margin:0 auto; position:relative;}
.category_wrap .tab ul{overflow:hidden; display:table; border:1px solid #DBDBDB; border-top:1px solid #DBDBDB; width:100%;}
.category_wrap .tab ul li{display:table-cell; box-sizing:border-box; background-color:#fff;}
.category_wrap .tab ul li a{display:block; height:58px; line-height:58px; position:relative; font-size:18px; color:#000; text-align:center;}
.category_wrap .tab ul li.active{background-color:#000;}
.category_wrap .tab ul li.active a{color:#fff;}
.category_wrap .tab ul li span{position:relative;}
.category_wrap .tab ul li.active span{padding-left:20px;}
.category_wrap .tab ul li.active span:before{position:absolute; content:''; left:0; top:50%; margin-top:-6px; width:14px; height:12px; background:url('../../images/biz/sub/icon_tab.png') no-repeat;}
.category_wrap .tab ul.col03 li{width:33.33%}
.category_wrap .tab ul.col05 li a{padding:0 20px;}
.category_wrap .tab ul.col07 li a{padding:0 20px;}

.form_checkbox{display:inline-block; cursor:pointer; overflow:hidden; position:relative; vertical-align:middle; color:#777; font-size:18px; line-height:25px;}
.form_checkbox input[type="checkbox"]{height:0; opacity:0; width:0; position:absolute; left:-10px;}
.form_checkbox .icon{position:relative; padding-left:30px;}
.form_checkbox .icon:before{position:absolute; content:''; top:4px; left:0; width:20px; height:20px; border-radius:4px; overflow:hidden; background:#fff url('../../images/biz/sub/icon_check.png') no-repeat;}
.form_checkbox input[type="checkbox"]:checked + .icon:before{background-position:-22px 0;}
.form_checkbox input[type="checkbox"]:checked + span{color:#000; font-weight:bold;}

/* category */
.category_wrap .pd_unit.cont02:before{background:#000;}
.category_wrap .pd_unit .tab{margin-bottom:40px;}
.category_wrap .pd_unit .category_slide{width:950px; margin:0 auto; padding-bottom:84px; position:relative;}
.category_wrap .pd_unit .category_slide ul{height:100%; overflow:hidden;}
.category_wrap .pd_unit .category_slide ul li{position:relative; width:215px; height:100%; padding:0 5px; box-sizing:border-box;}
.category_wrap .pd_unit .category_slide ul li a{display:block;}
.category_wrap .pd_unit .category_slide ul li a .thum{width:205px; height:205px; box-sizing:border-box;}
.category_wrap .pd_unit .category_slide ul li a .thum img{width:100%; height:100%;}
.category_wrap .pd_unit .category_slide ul li.round .thum{border:1px solid #DBDBDB; border-radius:50%; overflow:hidden; box-sizing:border-box;}
.category_wrap .pd_unit .category_slide ul li.round .thum img{width:100%; height:100%;}
.category_wrap .pd_unit .category_slide ul li .cont{padding:13px 7px 0;}
.category_wrap .pd_unit .category_slide ul li .brand{font-size:14px; color:#777;width:100%;line-height: 1.4em;text-overflow: ellipsis;white-space: nowrap;overflow: hidden;}
