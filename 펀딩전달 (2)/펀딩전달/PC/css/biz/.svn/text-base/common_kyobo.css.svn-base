.pagination_v2 .active{background:#000;;}
.pagination_v2 .arr{display:inline-block; width:20px; padding-top:12px;}
.pagination_v2 .arr:after{content:''; display:inline-block; width:15px; height:25px; background:url('../../images/biz/sub/icon_page_arr.png') no-repeat; background-size:60px 25px;}
.pagination_v2 .arr.prev{margin-right:50px;}
.pagination_v2 .arr.next{margin-left:50px;}
.pagination_v2 .arr.prev:after{background-position:-15px 0}
.pagination_v2 .arr.first:after{background-position:0 0}
.pagination_v2 .arr.next:after{background-position:-30px 0}
.pagination_v2 .arr.last:after{background-position:100% 0}

/* footer */
#footer{position:relative; border-top:1px solid #808080; background-color:#fff; min-width:1280px;}
#footer .util{position:relative; background-color:#fbfbfb; border-bottom:1px solid #f6f6f6;}
#footer .util_cont{position:relative; width:950px; height:48px; margin:0 auto;}
#footer .util_cont > ul{overflow:hidden; float:left;}
#footer .util_cont > ul li{position:relative; float:left; padding:17px 0 18px;}
#footer .util_cont > ul li:nth-child(3):after{position:absolute; content:''; top:10px; left:22px; width:4px; height:4px; border-radius:2px; overflow:hidden; background:#DA2128}
#footer .util_cont > ul li + li{padding-left:20px;}
#footer .util_cont > ul li + li:before{position:absolute; content:''; top:50%; margin-top:-6px; left:10px; background:#929292; width:1px; height:12px;}
#footer .util_cont > ul li a{display:block; color:#333; font-size:13px;}

#footer .util .family_site{position:absolute; top:0; right:0; z-index:20; width:200px; height:48px; color:#333; font-size:14px; cursor:pointer; background:#fbfbfb; border-right:1px solid #f6f6f6; border-left:1px solid #f6f6f6;}
#footer .util .family_site:after{position:absolute; content:''; display:block; right:25px; top:22px; width:11px; height:5px; background:url('../../images/biz/common/icon_footer_arr.png') no-repeat;}
#footer .util .family_site p{line-height:48px; text-indent:20px;}
#footer .util .family_site ul{display:none; position:absolute; left:-1px; right:-1px; top:48px; height:240px; overflow-y:auto; background:#fff; border:1px solid #f6f6f6;}
#footer .util .family_site ul li{background:#fff;}
#footer .util .family_site ul li a{display:block; padding:5px 20px; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
#footer .util .family_site ul li a:hover{background:#fbfbfb;}
#footer .util .family_site:hover ul{display:block;}

#footer .inner{position:relative; width:950px; margin:0 auto; padding:34px 0 87px; overflow:hidden;}
#footer .inner .cont{float:left;}
#footer .inner .cont.left{width:513px;}
#footer .inner .cont.right{width:437px;}
#footer .inner .cont .cs_center > strong{display:block; font-size:52px; font-weight:100;}
#footer .inner .cont .cs_center > span{display:block; color:#777; font-size:14px; margin:10px 0 18px;}
#footer .inner .cont .cs_center dl{overflow:hidden;}
#footer .inner .cont .cs_center dl dt{float:left;}
#footer .inner .cont .cs_center dl dt a{display:inline-block; width:103px; border:1px solid #000; color:#000; box-sizing:border-box; font-size:16px; text-align:center; vertical-align:middle;}
#footer .inner .cont .cs_center dl dt a span{display:inline-block; vertical-align:middle; height:42px; line-height:42px;}
#footer .inner .cont .cs_center dl dd{line-height:20px; padding:2px 0; margin-left:115px;}
#footer .inner .cont.left > h2{display:block; font-size:20px; margin:44px 0 10px;}
#footer .inner .cont.left > h2 + span{color:#000;}
#footer .inner .cont.left > span{display:block; color:#777; line-height:22px;}
#footer .inner .cont.left > span a{color:#777; margin-left:10px;}

#footer .inner .cont .sns{overflow:hidden; display:table; margin:6px 0 16px;}
#footer .inner .cont .sns li{display:table-cell; text-align:center; vertical-align:middle; width:68px; height:68px;}
#footer .inner .cont .sns li a{display:inline-block; text-align:center;}
#footer .inner .cont .sns li + li{padding-left:8px;}
#footer .inner .cont .mobile a{position:relative; display:inline-block; color:#fff; background:#000; width:182px; padding-left:30px; box-sizing:border-box; text-align:center; line-height:44px; height:44px;}
#footer .inner .cont .mobile a + a{margin-left:6px;}
#footer .inner .cont .mobile a:before{position:absolute; content:''; width:22px; height:22px; top:50%; margin-top:-11px; left:36px;}
#footer .inner .cont .mobile a.ios:before{background:url('../../images/biz/common/icon_ios.png') no-repeat; background-size:21px 22px;}
#footer .inner .cont .mobile a.and:before{background:url('../../images/biz/common/icon_google.png') no-repeat;}
#footer .inner .cont .copyright{overflow:hidden; margin:20px 0 27px;}
#footer .inner .cont .copyright li{float:left;}
#footer .inner .cont .copyright li + li{margin-left:70px;}
#footer .inner .cont.right > span{display:block; margin-bottom:2px;}
#footer .inner .cont .payment > *{vertical-align:top;}
#footer .inner .cont .payment .font_t{font-size:20px; font-weight:bold; margin-right:20px;}
#footer .inner .cont .payment a{font-weight:bold; line-height:20px; color:#000;}
#footer .inner .cont.right .txt{color:#777; line-height:22px;}
