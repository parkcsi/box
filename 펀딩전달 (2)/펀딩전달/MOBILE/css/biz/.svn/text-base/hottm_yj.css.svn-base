.record_wrap .recomm_wrap .desc_recomm em.rec_per {font-weight: 600;margin-top:7px;color: #ff6a5b;}
.record_wrap .recomm_wrap .list_recomm {padding-right:10px;}

.record_wrap .recomm_wrap .list_recomm {margin-top:20px;}
.record_wrap .recomm_wrap .list_recomm li {position: relative;float:left;width:calc(33.3333% - 5.5px);margin-right:8px;}
