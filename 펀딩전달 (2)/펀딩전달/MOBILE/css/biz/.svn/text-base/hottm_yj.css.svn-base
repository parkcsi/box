.record_wrap .sign_wrap .list_schedule li h3 {position: relative;padding-left:19px;font-weight: 400;font-size:1.167em;color:#ff6a5b;letter-spacing: -0.029em;display: -webkit-box;-webkit-line-clamp: 1;-webkit-box-orient:vertical;overflow: hidden;max-width: 90%;}
