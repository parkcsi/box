.record_wrap .magazine_wrap .mag-container .desc_magazine h4 {margin-top:10px;font-size:1.167em;font-weight: 400;color:#fff;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient:vertical;overflow: hidden; line-height: 19px; height: 38px;}
.record_wrap .magazine_wrap .mag-container .desc_magazine span {font-size:0.917em;color:#dbdbdb;margin-top:7px;margin-right: 7px;}
.record_wrap .magazine_wrap .mag-container .desc_magazine span strong {font-weight: normal; font-size: 1.1em; letter-spacing: 0.5px;}
/* 컬처이벤트 */
