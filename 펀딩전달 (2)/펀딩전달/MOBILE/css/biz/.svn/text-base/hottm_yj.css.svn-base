@charset "utf-8";

/* 접근성 */
.skip{z-index:4;position:absolute;left:50%;top:0;margin-left:-250px;width:500px;}
.skip>li{position:absolute;left:50%;margin-left:-250px;top:0;width:500px;height:auto;}
.skip>li>a{display:block;padding:0;font-size:0;text-align:center;background:#fff;color:#000;}
.skip>li>a:focus{padding:15px 0;font-size:16px;box-sizing: border-box;}
/* 공통 */
.hidden {display:none;position:absolute;font-size:0;width:0;height:0;line-height:0;background:none;}
.blind {text-indent:-2000px;overflow:hidden;}
.clear:after{content:'';display:block;clear:both;}
.content_new .loginWrap input[type="text"],input[type='password'],input[type='submit'],input[type='search'],input[type='number'] {-webkit-appearance: none;-moz-appearance: none;appearance: none;border-radius:0; font-family:'font_ns', 'AppleGothic', sans-serif;}
/* 모바일 header */
.header {width:100%;height:70px;background: #fff;position:fixed;z-index:20;}
.header_wrap {height:100%;}
.header_wrap .tit_header {height:100%;padding:0 100px;text-align:center;overflow:hidden;}
.header_wrap .tit_header span {display:inline-block;white-space:nowrap;overflow:hidden;}
.header_wrap .ic_header {position:absolute;width: 22px;height: 19px;overflow:hidden;top:25.5px;}
.header_wrap .ic_header a {display:block;width:100%;height:100%;background-position:50% 50%;background-repeat:no-repeat;background-size:100%;}
.header_wrap .ic_header.h_back {left:20px;}
.header_wrap .h_back a {background-image:url(../../images/biz/common/btn_back.png);}

/* 로그인 */
.content_new #content.cont_login {margin-top: 0;}
.loginWrap {position:relative;padding:70px 50px 52.5px;margin:0 auto;}
.loginWrap .logo_img {text-align: center;}
.loginWrap .logo_txt {text-align: left;}
.loginWrap .logo_txt p {font-size:3.00em;}
.loginWrap .tab_login {margin-top:39.74px;}
.loginWrap .tab_login.m_tab_login {margin-top:20px;}
.loginWrap .tab_login li {float:left;width:50%;text-align: center;}
.loginWrap .tab_login.m_tab_login li a {display:block;padding:12.5px 0;box-sizing:border-box;font-size:1.333em;}
.loginWrap a.btn_member_on, a.btn_nonmember_on {box-shadow: 0px 5px 10px #00000029;border: 1px solid #000;background: #fff;}
.loginWrap a.btn_member_off, a.btn_nonmember_off {border: 1px solid #eee;color:#999;background: #f8f8f8;}
/* IE input 초기화 */
input::-ms-clear, input::-ms-reveal {display: none;}
.loginWrap .m_inp_login{position: relative;margin-top:60px;}
.loginWrap .m_inp_login.m_inp_pw {margin-top:20px;}
.loginWrap .box_login legend{display:none;}
.loginWrap .m_login::-webkit-input-placeholder{color:#dbdbdb;font-size:1em;letter-spacing: 0.1em;font-weight: 400;}
.loginWrap .m_login::-moz-placeholder{color:#dbdbdb;font-size:1em;letter-spacing: 0.1em;font-weight: 400;}
.loginWrap .m_login:-moz-placeholder{color:#dbdbdb;font-size:1em;letter-spacing: 0.1em;font-weight: 400;}
.loginWrap .m_login:-ms-input-placeholder{color:#dbdbdb;font-size:1em;letter-spacing: 0.1em;font-weight: 400;}
.loginWrap .m_inp_login .m_login{width:100%;height:48px;background: #fff;border:none;border-bottom: 1px solid #000;font-size:1.667em;font-weight: 600;}
.loginWrap input.m_login:focus{border-bottom: 1px solid #ff6a5b;outline:none;}
.loginWrap .opt_login{margin-top:45px;text-align:right;}
.loginWrap .opt_login:after{content:'';display:block;clear:both;}
.loginWrap .opt_login .auto_save{float:right;}
.loginWrap .inp_chk{display:inline-block;overflow:hidden;position:relative;line-height:14px;vertical-align:middle;margin-left:17px;}
.loginWrap .inp_chk input[type="checkbox"]{position:absolute;top:1px;left:1px;width:0;height:0;-webkit-appearance:none;-moz-appearance: none;appearance: none;outline: 0;background: transparent;}
.loginWrap .inp_chk label,.inp_chk span{display:inline-block;position:relative;padding-left:24px;font-size:1em;color:#777;padding-top: 2px;padding-bottom:3px;box-sizing: border-box;}
.loginWrap .inp_chk label:before,.inp_chk span:before{content:'';display:block;position:absolute;top:1px;left:1px;width:18px;height:18px;background:url(../../images/biz/sub/btn_check_off.png) no-repeat;border-radius:4px;background-position: 0 0;background-size:100%;box-sizing: border-box;border:1px solid #dbdbdb;}
.loginWrap .inp_chk input[type="checkbox"]:checked + label:before,.inp_chk input[type="checkbox"]:checked + span:before{background:url(../../images/biz/sub/btn_check_on.png) no-repeat;background-size:100%;border:none;}
.loginWrap .btn_login{display:block;width:100%;margin-top:15px;padding:11.5px 0;font-size:1.667em;font-family:'font_ns', 'AppleGothic', sans-serif;}
.loginWrap .btn_login.off{color:#000;border: 1px solid #000;background: #fff;box-sizing: border-box;}
.loginWrap .btn_login.on{color:#fff;border:0;background: #000;padding:12.5px 0;}
.loginWrap p.txt_inquiry {color:#777;font-size:1em;margin-top:45px;letter-spacing: -0.04em;}
/* 아이디찾기 등 */
.loginWrap .list_loginEtc {margin-top:15px;padding-bottom:40px;border-bottom:1px solid #eee;}
.loginWrap .list_loginEtc li {float:left;width:33.33333%;text-align: center;}
.loginWrap .list_loginEtc li a {display: block;font-size:1em;color: #777;padding:5px 0 10px;}
.loginWrap .list_loginEtc li a.btn_register {color: #000;}
.loginWrap .login_naver {margin-top:30px;text-align: center;}
.loginWrap .login_naver a {font-size:1.167em;}
.loginWrap .login_naver a i {display: inline-block;padding-top:2.5px;font-style:normal;}
.loginWrap .login_naver a img {width:26px;height:26px;margin-right:3.8px;}
/* 로그인 하단배너 */
.loginWrap .bnr_login {display:table;width:100%;height: 120px;background: #f4f2f5;margin-top:33px;}
.loginWrap .bnr_login a {display: table-cell;vertical-align: middle;width:100%;height:100%;text-align: center;font-size:1.667em;color:#dbdbdb;}
.loginWrap .bnr_login a img {max-width:100%;max-height:120px;}
.loginWrap .txt_customer {font-size:1.667em;text-align: center;letter-spacing: -1px;line-height: 30px;font-weight: 100;color: #000;margin-top: 144px;}
.loginWrap .txt_customer>span {display: inline-block;color:#000;font-weight: 100;font-size:1.100em;}
.loginWrap .txt_center {font-size:0.917em;text-align: center;letter-spacing: -0.044em;line-height: 30px;color: #666;}
.loginWrap span.bt_bar {display: block;width:100%;height:1px;background: #eee;margin:10px 0;}
.loginWrap p.copyright {font-size:0.75em;color:#999;text-align: center;word-break: keep-all;}
/* 가입 */
.loginWrap .box_register {margin-top:45px;}
.loginWrap .box_register>p {letter-spacing: -0.020em;color: #777;font-size:1em;line-height: 16px;margin-top:8.5px;}
.loginWrap a.link_register {font-size:1.167em;color: #000;letter-spacing: -0.28px;}
.loginWrap a.link_register img {width:8px;height:14px;margin-left: 4px;padding-top: 4px;}
.loginWrap .search,.searchClear {position: absolute;;right: 10px;top: 18px;width: 12.5px;height: 12.5px;cursor: pointer;}
.loginWrap .search img,.searchClear img {width:100%;}
/* 오늘 혜택 */
.content_new.today #header {z-index: 98;}
#benefit_wrapper {z-index: 99;position: fixed;left: 0;top: 100%;bottom: 0;width: 100%;padding-bottom: 180px;background: #fff;box-sizing: border-box;overflow:auto;}
#benefit_wrapper > .swiper-container {width: 100%;height: 100%;box-sizing: border-box;}
#benefit_wrapper > .swiper-container .swiper-slide {height: auto !important;}
#app_bar .btn_today.active {background: url(../../images/biz/common/appbar_close.png) no-repeat;background-size: 100%;}
#app_bar .img_today {position: absolute;z-index: 99;content: '오늘,혜택';display: block;color: #fff;left: 50%;margin-left: -34px;top: -35px;width: 69px;height: 36px;padding-top: 6px;text-align: center;text-indent: initial;box-sizing: border-box;background: url(../../images/biz/common/icon_tooltip.png) no-repeat;background-size: 69px 36px;}
#app_bar .img_today.none {display:none;}

