.record_wrap .recomm_wrap .seemore_wrap small {display:inline-block;font-size: 1.333em;color:#fff;font-weight: 200;}
.record_wrap .recomm_wrap .seemore_wrap small b {display:inline-block;font-weight: 600;color:#ff6a5b;}
/* 뮤직매거진 */
