/* 히스토리 */
#history_wrapper {z-index: 999;position: fixed;left: 100%;top: 0;bottom: 0;width: 100%;background: #fff;box-sizing: border-box;overflow:hidden;overflow-y: auto;}
#history_wrapper > .swiper-container {width: 100%;height: 100%;box-sizing: border-box;}
#history_wrapper > .swiper-container .swiper-slide {height: auto !important;}

.top_header {position: relative;left: 0;top: 0;border-bottom: 1px solid #DBDBDB;background-color: #fff;}
.top_header .btn_close {position: absolute;right: 15px;top: 10px;width: 40px;height: 40px;text-indent: -9999px;font-size: 0;background: url(../../images/biz/common/btn_close_m.png) no-repeat 50% 50%;background-size: contain;}
.top_header h2 {font-size: 1.667em;padding: 0 80px;overflow: hidden;text-align: center;line-height: 60px;box-sizing: border-box;white-space: nowrap;text-overflow: ellipsis;}
.history_inner {width:100%;padding:40px 30px 50px;}
.history_inner .category_type {position:relative;margin-right: 30px;margin-bottom:42px;}
.history_inner .category_type .btn_histShop, .btn_histGood {font-size: 1.667em;font-weight: 200;color:#777;letter-spacing: -0.1em;margin-right: 20px;}
.history_inner .category_type a.active {font-weight: 600;color:#000;}
.history_inner .category_type .txt_blue {position:absolute;right:20px;top:7px;}
.history_inner .category_type .txt_blue a {display: block;}
.history_inner .category_type .txt_blue a em {position: relative;color:#49A4FE;font-size:1.09em;letter-spacing: normal;padding-right: 20px;}
.history_inner .category_type .txt_blue a em.edit:before {content:'';position: absolute;right:0;top:2px;width:16px;height:16px;background: url(../../images/biz/sub/history/ic_edit.png) no-repeat;background-size:100%;}
.history_inner .category_type .txt_blue a em.cancel:before {content:'';position: absolute;right:0;top:2px;width:16px;height:16px;background: url(../../images/biz/sub/history/ic_cancel.png) no-repeat;background-size:100%;}
.history_inner .category_type .bott_border {display: block;width:100%;height:2px;margin-top:18px;background: #000;}

.history_inner .category_type .list_kind {margin-top:10px;margin-right: 20px;}
.history_inner .category_type .list_kind li {position: relative;float:left;width:calc(20% - 5.5px);margin-right: 6.5px;height: 50px;text-align: center;border: 1px solid #dbdbdb;border-radius: 5px;padding-top: 5px;box-sizing: border-box;}
.history_inner .category_type .list_kind li.active {border: 1px solid #000;box-shadow: 0px 4px 5px rgba(0,0,0, .16);}
.history_inner .category_type .list_kind li:nth-child(5) {margin-right: 0;padding-top: 2px;}
.history_inner .category_type .list_kind li i {display: block;margin:0 auto;}
.history_inner .category_type .list_kind li i.ic_01 {width:17px;height:20px;background: url(../../images/biz/sub/history/ic_history_01_off.png) no-repeat;background-size: contain;}
.history_inner .category_type .list_kind li i.ic_02 {width:20px;height:20px;background: url(../../images/biz/sub/history/ic_history_02_off.png) no-repeat;background-size: contain;}
.history_inner .category_type .list_kind li i.ic_03 {width:20px;height:20px;background: url('http://image.kyobobook.co.kr/newimages/giftshop_new/resources/img/common/ic_history_03_off.png') no-repeat;background-size: contain;}
.history_inner .category_type .list_kind li i.ic_04 {width:22px;height:23px;background: url(../../images/biz/sub/history/ic_history_04_off.png) no-repeat;background-size: contain;}
.history_inner .category_type .list_kind li i.ic_05 {width:22px;height:23px;background: url(../../images/biz/sub/history/ic_history_05_off.png) no-repeat;background-size: contain;}
.history_inner .category_type .list_kind li.active i.ic_01 {background: url(../../images/biz/sub/history/ic_history_01_on.png) no-repeat;background-size: contain;}
.history_inner .category_type .list_kind li.active i.ic_02 {background: url(../../images/biz/sub/history/ic_history_02_on.png) no-repeat;background-size: contain;}
.history_inner .category_type .list_kind li.active i.ic_03 {background: url('http://image.kyobobook.co.kr/newimages/giftshop_new/resources/img/common/ic_history_03_on.png') no-repeat;background-size: contain;}
.history_inner .category_type .list_kind li.active i.ic_04 {background: url(../../images/biz/sub/history/ic_history_04_on.png) no-repeat;background-size: contain;}
.history_inner .category_type .list_kind li.active i.ic_05 {background: url(../../images/biz/sub/history/ic_history_05_on.png) no-repeat;background-size: contain;}
.history_inner .category_type .list_kind li span {position: absolute;left:0;bottom:4px;width:100%;font-size:1em;color:#777;}
.history_inner .category_type .list_kind li.active span {color:#000;}
.history_inner .category_type .list_kind li a {position: absolute;left:0;top:0;width:100%;height:100%;}

.history_inner .box_history {position: relative;margin-right: 30px;padding-bottom: 47px;text-align: center;}
.history_inner .box_history .tdy {text-align: center;font-size:1.667em;font-weight:600;margin-bottom:50px;}
.history_inner .box_history .lst {position: relative;display: inline-block;margin-left: -30px;text-align: center;font-size:1.333em;font-weight:bold;margin-bottom:50px;letter-spacing: 0.2em;}
.history_inner .box_history .lst .year {position: relative;color:#DBDBDB;}
.history_inner .box_history .lst .year:before {position: absolute;content: '';right: -9px;top: -1px;width: 9px;height: 2px;background: #dbdbdb;transform: rotate(-49deg);}
.history_inner .box_history .lst em {position: relative;font-weight: bold;}
.history_inner .box_history .lst em:before {position: absolute;content: '';left: -8px;top: 0;width: 9px;height: 2px;background: #000;transform: rotate(-49deg);}
.history_inner .box_history .lst i {position:absolute;right: -33px;top:0;line-height: 20px;display:inline-block;font-style:normal;font-family:font_ns;color:#fff;width:19px;height:19px;border-radius: 50%;background: #000;}
.history_inner .box_history .lst i:before {position: absolute;left: 0;top: 0;line-height: 1.1em;padding-left: 2.3px;}
.history_inner .box_history .lst i.sun:before {content:'o';color:#FF6A5B;padding-left: 4.5px;line-height: 1em;}
.history_inner .box_history .lst i.mon:before {content:'o';padding-left: 4.5px;line-height: 1em;}
.history_inner .box_history .lst i.tue:before {content:'ㅎ';}
.history_inner .box_history .lst i.wed:before {content:'ㅅ';}
.history_inner .box_history .lst i.thur:before {content:'ㅁ';}
.history_inner .box_history .lst i.fri:before {content:'ㄱ';}
.history_inner .box_history .lst i.sat:before {content:'ㅌ';}
.history_inner .box_history:before {content:'';position: absolute;left: 0;top:0;width:1px;height: 100%;background: #000;}
.history_inner .box_history:after {content:'';position: absolute;left:-5px;top:0;width:10px;height: 10px;background: #000;border-radius:50%;}
.history_inner .box_history.pd_list ul {margin-top:0;margin-left: 22px;border-top:1px solid #dbdbdb;overflow: visible;text-align: left;}
.history_inner .box_history.pd_list ul li {position: relative;float: none;padding-left:0;width:100%;margin-top:-1px;border-top:1px solid #dbdbdb;border-bottom:1px solid #dbdbdb;}
.history_inner .box_history.pd_list ul li.ic_03.sold_out:after {content:'';position: absolute;left:0;top:0;width:100%;height:100%;}
.history_inner .box_history.pd_list ul li.ic_04.sold_out:after {content:'';position: absolute;left:0;top:0;width:100%;height:100%;}

.history_inner .catalog_history li i {position: absolute;display: inline-block;margin:10px 0;width:64px;height: 64px;border-radius: 50%;border: 1px solid #DBDBDB;overflow: hidden;}
.history_inner .catalog_history li i img{width:100%;height:100%;}
.history_inner .catalog_history li .btn_like{top:50%;}
.history_inner .catalog_history li .desc_type01 {position: relative;display: inline-block;max-width: 63%;padding:10px 0 10px 74px;}
.history_inner .catalog_history li .desc_type01 h2 {font-weight: 400;font-size:1.167em;height: 40px;margin-bottom:7px;word-break: keep-all;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;overflow: hidden;}
.history_inner .catalog_history li .desc_type01 p span {font-size:1.167em;font-weight: 600;margin-right: 10px;color:#000;}
.history_inner .catalog_history li .desc_type01 p span.percent {color:#DA2128;}
.history_inner .catalog_history li .desc_type02 {display:block;width:100%;padding:20px 0;}
.history_inner .catalog_history li .desc_type02 h2 {font-size:1.167em;font-weight: 400;max-width: 95%;white-space: nowrap;text-overflow: ellipsis;overflow: hidden;}
.history_inner .catalog_history li .desc_type02 p {font-size:1em;color:#777;margin-top: 2px;max-width: 95%;white-space: nowrap;text-overflow: ellipsis;overflow: hidden;}

.history_inner .catalog_history li.hide {display: none;}
.history_inner .catalog_history li.ic_01:before {content: '';position: absolute;left:-28px;top:50%;margin-top:-15px;width:12.5px;height:31px;background:#fff url(../../images/biz/sub/history/ic_history_01_on.png) no-repeat 50%;background-size:contain;}
.history_inner .catalog_history li.ic_02:before {content: '';position: absolute;left:-29px;top:50%;margin-top:-15px;width:14.5px;height:31px;background:#fff url(../../images/biz/sub/history/ic_history_02_on.png) no-repeat 50%;background-size:contain;}
.history_inner .catalog_history li.ic_03:before {content: '';position: absolute;left:-30px;top:50%;margin-top:-15px;width:19px;height:31px;background:#fff url('http://image.kyobobook.co.kr/newimages/giftshop_new/resources/img/common/ic_history_03_on.png') no-repeat 50%;background-size:contain;}
.history_inner .catalog_history li.ic_04:before {content: '';position: absolute;left:-33px;top:50%;margin-top:-16px;width:18px;height:32px;background:#fff url(../../images/biz/sub/history/ic_history_04_on.png) no-repeat 50%;background-size:contain;}
.history_inner .catalog_history li.ic_05:before {content: '';position: absolute;left:-30px;top:50%;margin-top:-17px;width:18px;height:32px;background:#fff url(../../images/biz/sub/history/ic_history_05_on.png) no-repeat 50%;background-size:contain;}

.history_inner .catalog_history li.chk_off .chk_delete {content: '';position: absolute;top: 50%;left: -30px;margin-top: -9px;width: 18px;height: 18px;background: url(../../images/biz/sub/btn_check_off.png) no-repeat;border-radius: 4px;background-position: 0 0;background-size: 100%;box-sizing: border-box;border: 1px solid #dbdbdb;}
.history_inner .catalog_history li.chk_on .chk_delete {background: url(../../images/biz/sub/btn_check_on.png) no-repeat;background-size: 100%;border: none;}
/* 히스토리 품절 */
.history_inner .catalog_history li .run_out {display: none;}
.history_inner .catalog_history li.sold_out .run_out {z-index: 103;position: absolute;display: block;top: 0;right: 5px;height: 100%;width: 40px;text-align: center;font-size: 1.167em;background: #fff;}
.history_inner .catalog_history li.sold_out .run_out span {display: none;position: absolute;left: 0;top: 50%;width:100%;margin-top:-0.7em;vertical-align: middle;color:#777;}
.history_inner .catalog_history li.sold_out .run_out.out01 span.out01 {display: table-cell;margin-top: -1.45em;}
.history_inner .catalog_history li.sold_out .run_out.out02 span.out02 {display: table-cell;}
.history_inner .catalog_history li.sold_out .run_out.out03 span.out03 {display: table-cell;}
.history_inner .catalog_history li.sold_out i {opacity: 0.5;}
.history_inner .catalog_history li.sold_out .desc_type01 h2,
.history_inner .catalog_history li.sold_out .desc_type02 h2,
.history_inner .catalog_history li.sold_out .desc_type02 p,
.history_inner .catalog_history li.sold_out .desc_type01 p span,
.history_inner .catalog_history li.sold_out .desc_type01 p span.percent {color:#dbdbdb;}
/* 히스토리 삭제 */
.category_type .list_erase {margin-top:10px;margin-right: 20px;border:1px solid #000;}
.category_type .list_erase li {position: relative;float:left;width:50%;height:50px;text-align: center;}
.category_type .list_erase li span {font-size:1.333em;line-height: 49px;}
.category_type .list_erase li a {position: absolute;left:0;top:0;width:100%;height: 100%;}
.category_type .list_erase li:nth-child(1) img {width: 12px;vertical-align: baseline;margin-right: 5px;}
.category_type .list_erase li:nth-child(2) img {width: 14px;vertical-align: text-top;margin-right: 3px;}
.category_type .list_erase li:nth-child(2) {border-left:1px solid #000;box-sizing: border-box;}
/* 히스토리 없음 */
.history_inner .vacant_history {text-align: center;}
.history_inner .vacant_history img {width: 135px;}
.history_inner .vacant_history img.login {width:150px;}
.history_inner .vacant_history img.brand {width:150px;}
.history_inner .vacant_history p {font-size:1.333em;word-break: keep-all;margin:28px auto 70px;}
.history_inner .vacant_history p span {color:#3A4152;}
.history_inner .vacant_history a {display: block;font-size:1.333em;color:#fff;height:50px;line-height: 49px;background: #000;text-align: center;margin: 0 40px;}
/* 히스토리 좋아요 */
.history_inner.like_inner {padding:0;padding-bottom:180px;}
.history_inner.like_inner .category_type {padding:40px 0 0 30px;margin-right: 0;margin-bottom:31px;}
.history_inner .list_history {margin-top:15px;}
.history_inner .list_history li {float:left;margin-right: 15px;}
.history_inner .list_history li a {position: relative;padding-left:14px;font-size:1.333em;font-weight:200;}
.history_inner .list_history li.on a {font-weight:600;}
.history_inner .list_history li a:before {content:'';position: absolute;left:1px;top:50%;margin-top:-2px;width:5px;height:5px;background: url(../../images/biz/common/chk_type_off.png) no-repeat;background-size:100%;}
.history_inner .list_history li.on a:before {left:-1px;width:10px;height:9.5px;margin-top: -3px;background: url(../../images/biz/common/chk_type.png) no-repeat;background-size:contain;}
.history_inner .pd_list.coupon ul {margin-top: -50px;}
.history_inner .topValue .pd_list {padding-bottom:55px;}
.history_inner .topValue .choice_wrap {padding-top:0;padding-bottom:20px;}

.history_inner .plus_spot {margin-top:100px;}
.history_inner .plus_spot h2 {font-size: 2.250em;margin-bottom:33px;padding-left: 30px;font-weight: 400;letter-spacing: -0.025em;}
.history_inner .plus_spot .swiper-slide .thum em {position: absolute;top: 0;right: 0;left: 0;bottom: 0;padding: 65px 0;text-align: center;color: #fff;font-size: 1.667em;font-weight: 400;background: rgba(0,0,0,0.6);}
.history_inner .plus_spot .swiper-slide .thum em small {display:block;line-height: 1.3em;font-family: font_ns;font-size: 1em;}
.history_inner .swiper-container.plus_history {padding-bottom: 40px;}
.history_inner .swiper-container.plus_history .info.bigger {height: 38px;}
.history_inner .swiper-container.plus_history .info.bigger .like {display: block;width: 34px;height: 18.7px;padding: 30px 0 0 0;box-sizing: border-box;background-size: 20px 18.7px;text-align: center;background-position: center;margin: 0 auto;}

.history_inner .brand_history {padding-left: 30px;padding-bottom: 50px;}
.history_inner .brand_history .list_brands li {position: relative;width:100%;margin-bottom:15px;}
.history_inner .brand_history .list_brands li .txt_brand{display: inline-block;max-width: 55%;margin-top: 10px;}
.history_inner .brand_history .list_brands li .txt_brand h3 {font-size: 1.167em;width: 100%;text-overflow: ellipsis;overflow: hidden;white-space: nowrap;}
.history_inner .brand_history .list_brands li .txt_brand p {font-size: 1.167em;font-weight: 600;font-weight: 600;text-overflow: ellipsis;overflow: hidden;white-space: nowrap;}
.history_inner .brand_history .list_brands li .like_brand{position: absolute;right:25px;top:50%;margin-top: -22px;width: 34px;}
.history_inner .brand_history .list_brands li i{float:left;display:inline-block;width:60px;height: 60px;margin-right: 10px;overflow: hidden; border: 1px solid #dbdbdb; box-sizing: border-box; border-radius: 50%;}
.history_inner .brand_history .list_brands li i img{width:100%;height:100%;}
.history_inner .brand_history .list_brands li a{display:block;width:100%;}
.history_inner .leftValue .choice_wrap{padding-top:0;padding-bottom: 20px;}

.history_inner .brand_history .list_brands .like_brand .like {z-index: 10;display: block;height: 18.7px;font-family: font_gs;font-size: 1em;font-weight: 600;padding: 30px 0 0 0;box-sizing: border-box;text-align: center;background-position: center;background: url(../../images/biz/sub/icon_like.png) 50% 50% no-repeat;background-size: 20px;}
.history_inner .brand_history .list_brands .like_brand .like.on {background: url(../../images/biz/sub/icon_like_on.png) 50% 50% no-repeat;background-size: 20px;}
.history_inner .brand_history .list_brands .like_brand a {position: absolute;left:0;top:0;width:100%;height:150%;}

/* 이럴 때 */
/* 이럴 때 한정 폰트 */
@font-face {
  font-family:font_ms;
  font-weight:400;
  src:local(※), url(../../font/NanumMyeongjo-Regular.woff) format('woff');
}
#header .top_menu .swiper-slide.active a .this {position: absolute;right: -5px;top: 3px;width: 18px;height: 15px;background: url(../../images/biz/sub/img_wink.png) no-repeat;background-size: contain;}
.life_wrapper{color:#000; font-family:font_ms, 'Nanum Myeongjo', sans-serif;}
.life_wrapper .life_inner {padding-bottom:50px;}
.life_wrapper .life_inner:nth-last-of-type(1) {padding-bottom:0;}
.life_wrapper .life_inner img {width:100%;}
.life_wrapper .life_inner .desc_txt {padding-left:48px;padding-bottom: 83px;}
.life_wrapper .life_inner.ver01 .desc_txt {margin-top:8px;}
.life_wrapper .life_inner.ver02 .desc_txt {margin-top:48px;}
.life_wrapper .life_inner.ver02 b {display: block;margin-right:40px;height:8px;background: #000;}
.life_wrapper .life_inner .desc_txt h2 {margin-bottom:13px;font-size:2.250em;font-weight: 400;text-overflow: ellipsis;white-space: nowrap;overflow: hidden;}
.life_wrapper .life_inner .desc_txt p {padding-right: 20px;font-size:1.167em;line-height: 1.429em;font-weight: 400;color:#777;}

.life_filter {position: relative;width:100%;height:60px;box-shadow: 1px 1px 2px #DBDBDB;}
.life_filter .pd_filter {margin-top:0;}
.life_filter .pd_filter .tag_list {height: 60px;padding:17px 10px 17px 40px;box-sizing: border-box;}
.life_filter .pd_filter .tag_list ul li {margin:0 4px;}
.life_filter .pd_filter .tag_list ul li a {font-weight: 400;padding: 6px 24px 5px 13px;}
.life_filter .pd_filter .tag_list .btn_sympathy {position: absolute;right: 18px;top: 50%;margin-top: -0.857em;font-size: 1.167em;padding: 5px 0 5px 20px;}
.life_filter .pd_filter .tag_list .btn_sympathy.active {color:#FF6A5B;}
.life_filter .pd_filter .tag_list .btn_sympathy:before {content:'';position: absolute;left:0;top:5px;width:16px;height:16px;background: url(../../images/biz/sub/ico_sympathy.png) no-repeat;background-size:contain;}
.life_filter .pd_filter .tag_list .btn_sympathy.active:before {background: url(../../images/biz/sub/ico_sympathy_on.png) no-repeat;background-size:contain;}

.life_wrapper .life_inner .desc_txt .like_heart {position: relative;float: right;width: 32px;margin-right: 19.5px;margin-top:7px;}
.life_wrapper .life_inner .desc_txt .like_heart .heart {display: block;width: 32px;font-size: 1.167em;letter-spacing: -0.03em;padding: 36px 0 0 0;box-sizing: border-box;text-align: center;background-position: center;background: url(../../images/biz/sub/heart_off.gif) 50% 0% no-repeat;background-size: 32px;}
.life_wrapper .life_inner .desc_txt .like_heart .heart.on {/*background: url(../../images/biz/sub/heart_on.gif) 50% 0% no-repeat;*/background-size: 32px;}
.life_wrapper .life_inner .desc_txt .like_heart a {position: absolute;left:0;top:0;bottom:0;width:100%;height:100%;}

.life_wrapper .life_inner .life_suggest {padding-left: 40px;}
.life_wrapper .life_inner .life_suggest .life-container {width:auto;}
.life_wrapper .life_inner .life_suggest .life-container .swiper-slide {width:200px;}
.life_wrapper .life_inner .life_suggest .life-container .img_life img {width:200px;height:340px;}

.life_wrapper .life_inner .life_suggest .life-container .txt_life {margin-top:20px;}
.life_wrapper .life_inner .life_suggest .life-container .txt_life h2{font-size:1.333em;line-height:1.250em;height:38px;text-overflow: ellipsis;max-width: 200px;overflow: hidden;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;overflow: hidden;}
.life_wrapper .life_inner .life_suggest .life-container .txt_life p {margin-top: 5px;font-size:1.167em;line-height:1.250em;color:#777;text-overflow: ellipsis;max-width: 200px;overflow: hidden;white-space: nowrap;}

.life_wrapper .life_inner .life_suggest .only_one {position: relative;left:calc(100% - 220px);width:200px;}
.life_wrapper .life_inner .life_suggest .only_one a {position: absolute;left:0;top:0;width:100%;height:100%;}
.life_wrapper .life_inner .life_suggest .only_one img {width:200px;height:340px;}
.life_wrapper .life_inner .life_suggest .only_one .txt_life {margin-top:20px;width:200px;}
.life_wrapper .life_inner .life_suggest .only_one .txt_life h2{font-size:1.333em;line-height:1.250em;height:38px;text-overflow: ellipsis;max-width: 200px;overflow: hidden;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;overflow: hidden;}
.life_wrapper .life_inner .life_suggest .only_one .txt_life p {margin-top: 5px;font-size:1.167em;line-height:1.250em;color:#777;text-overflow: ellipsis;max-width: 200px;overflow: hidden;white-space: nowrap;}
/* 설정 */
.setup_wrap {position: relative;padding:40px 0 0 50px;}
.setup_wrap .btn_check{position:absolute;top: 50%;margin-top: -16px;right:20px;display:inline-block;width:50px;height: 32px;border-radius:18px;background: #F8F8F8;border: 2px solid #eee;box-sizing: border-box;}
.setup_wrap .btn_check.set_on {background:#FF6A5B;border: 2px solid #FF6A5B;}
.setup_wrap .btn_check > span{position:absolute;top:0;left:0;width:28px;height:28px;border-radius: 32px;box-shadow: 0px 4px 8px #0000004D;background:#fff;transition:0.2s;-webkit-tap-highlight-color: transparent;}
.setup_wrap .btn_check > input:checked + span{transform: translateX(64%);transition: 0.2s;}
.setup_wrap .btn_check > input{opacity:0;}

.setup_wrap .setup_inner span{display:inline-block;font-size:1em;color:#ff6A5b;}
.setup_wrap .setup_inner small{display:inline-block;font-size:1em;color:#777;margin-left: 8px;}
.setup_wrap .setup_inner .setup_list {margin-bottom: 35.5px;}
.setup_wrap .setup_inner .setup_list li {position: relative;padding:22px 20px 18px 0;box-sizing: border-box;border-bottom:1px solid #eee;}
.setup_wrap .setup_inner .setup_list li.access_set:after {content:'';position: absolute;right:20px;top:50%;margin-top:-6px;width:8px;height:12px;background: url(../../images/biz/sub/icon_agit_arr.png) no-repeat;background-size:contain;}
.setup_wrap .setup_inner .setup_list li .btn_access {position: absolute;left:0;top:0;bottom:0;width:100%;height:100%;}
.setup_wrap .setup_inner .setup_list li h3 {display: inline-block;font-size:1.333em;font-weight:normal;max-width: 69%;text-overflow: ellipsis;white-space: nowrap;overflow: hidden;}
.setup_wrap .setup_inner .setup_list li p {width: 74%;color: #777;letter-spacing: -0.1em;line-height: 1.333em;padding-top:5px;word-break: keep-all;}
.setup_wrap .setup_inner .setup_list li p em {color:#49A4FE;}
.setup_wrap .setup_inner .setup_list li .btn_logout,
.setup_wrap .setup_inner .setup_list li .btn_delete {position: absolute;right:20px;top: 50%;margin-top: -15px;padding: 6px 10px;box-sizing: border-box;font-size:1.167em;width:74px;height:34px;text-align: center;border: 1px solid #777;background: #fff;}
/* 이벤트뷰 */
.category_wrap .pd_list.pd_best.pd_one {padding-bottom:0;}
.category_wrap .pd_list.pd_best.pd_one ul {margin-top:22px;}
.category_wrap .pd_list.pd_best.pd_one ul li:before {display: none;}
.category_wrap .pd_list.pd_best.pd_one ul li {width: 100%;margin-top:0;}
/* 알림 */
.alarm_wrap {position:relative;}
.alarm_wrap .notice_inner .alarm_set{display: block;}
.alarm_wrap.case01 .notice_inner .btn_edit{display: block;}
.alarm_wrap.case02 .notice_inner .btn_cancel{display: block;}
.alarm_wrap .notice_inner {padding: 16px 17px;background: #F9F9F9;}
.alarm_wrap .notice_inner p{float: left;font-size: 1em;letter-spacing: -0.100em;}
.alarm_wrap .notice_inner a{display: none;position: relative;float: right;color: #49A4FE;font-size: 1.083em;margin-left: 16px;}
.alarm_wrap .notice_inner span{position: relative;color: #49A4FE;padding: 0 19px 2px 2px;box-sizing: border-box;}
.alarm_wrap .notice_inner .btn_cancel:before{content: '';position: absolute;right: 0;top: 2px;width: 16px;height: 16px;background: url(../../images/biz/sub/history/ic_cancel.png) no-repeat;background-size: contain;}
.alarm_wrap .notice_inner .btn_edit:before{content: '';position: absolute;right: 0;top: 2px;width: 16px;height: 16px;background: url(../../images/biz/sub/history/ic_edit.png) no-repeat;background-size: contain;}
.alarm_wrap .notice_inner .alarm_set:before{content: '';position: absolute;right: 0;top: 2px;width: 15px;height: 15px;background: url(../../images/biz/sub/ic_alarm.png) no-repeat;background-size: contain;}

.alarm_wrap .alarm_inner {}
.alarm_wrap.case01 .alarm_inner .alarm_list {padding-left: 60px;}
.alarm_wrap.case02 .alarm_inner .alarm_list {padding-left: 35px;}
.alarm_wrap .alarm_inner .alarm_list li {position: relative;border-bottom:1px solid #eee;}
.alarm_wrap.case01 .alarm_inner .alarm_list li:before {content:'';position: absolute;left: -24px;top:22px;width:8px;height:8px;background: #DBDBDB;border-radius: 50%;-webkit-border-radius:50%;}
.alarm_wrap .alarm_inner .alarm_list li h2 {min-height: 30px;font-size:1.333em;letter-spacing: -0.1em;font-weight: 400;margin-bottom:5px;text-overflow: ellipsis;display:-webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;overflow: hidden;}
.alarm_wrap .alarm_inner .alarm_list li.end h2 {color:#777;}
.alarm_wrap .alarm_inner .alarm_list li p {font-size:1em;color:#777;}
.alarm_wrap .alarm_inner .alarm_list li.end p {display: none;}
.alarm_wrap .alarm_inner .alarm_list li span {display: none;font-size:1em;color:#FF6A5B;}
.alarm_wrap .alarm_inner .alarm_list li.end span {display:block;}
.alarm_wrap.case01 .alarm_inner .alarm_list li a.btn_alarm {display:block;padding: 13px 20px 15px 0;box-sizing: border-box;}
.alarm_wrap.case02 .alarm_inner .alarm_list li a.btn_alarm {display:block;padding: 13px 20px 15px 0;box-sizing: border-box;margin-left: 25px;}
.alarm_wrap.case01 .alarm_inner .alarm_list li a.chk_delete {display: none;}
.alarm_wrap.case02 .alarm_inner .alarm_list li a.chk_delete {display: block;position: absolute;top:16px;left: -5px;margin-left:0;width: 18px;height: 18px;background: url(../../images/biz/sub/btn_check_off.png) no-repeat;border-radius: 2px;-webkit-border-radius: 2px;background-position: 0 0;background-size: 100%;box-sizing: border-box;border: 1px solid #dbdbdb;padding: 0;}
.alarm_wrap.case02 .alarm_inner .alarm_list li.check_on  a.chk_delete {background: url(../../images/biz/sub/btn_check_on.png) no-repeat;background-size: 100%;border: none;}
.alarm_wrap .alarm_chk {position: absolute;display: inline-block;line-height: 14px;vertical-align: middle;overflow: hidden;}

.alarm_wrap .alarm_inner .box_delete {display: none;margin:20px 20px 18px 30px;}
.alarm_wrap.case02 .alarm_inner .box_delete {display: block;}
.alarm_wrap .alarm_inner .box_delete li {position: relative;width:50%;height: 52px;float:left;border: 1px solid #000;box-sizing: border-box;}
.alarm_wrap .alarm_inner .box_delete li:nth-child(2) {margin-left: -1px;}
.alarm_wrap .alarm_inner .box_delete li a {position: absolute;left: 0;top: 0;bottom: 0;width: 100%;height: 100%;font-size: 1.333em;text-align: center;}
.alarm_wrap .alarm_inner .box_delete li a.btn_part span {line-height: 49px;background:url(../../images/biz/common/chk_black.png) no-repeat 0 50%;background-size:9.5px 7px;padding-left: 14px;}
.alarm_wrap .alarm_inner .box_delete li a.btn_all span {line-height: 49px;background:url(../../images/biz/common/ic_delete.png) no-repeat 0 50%;background-size:12px 14px;padding-left: 16px;}

.alarm_wrap .no_alarm {text-align: center;padding-top:62px;}
.alarm_wrap .no_alarm img{width:97px;}
.alarm_wrap .no_alarm p{font-size: 1.333em;word-break: keep-all;margin: 28px auto 70px;}
.alarm_wrap .no_alarm p span{color: #424A5D;}
.alarm_wrap .no_alarm a{display: block;font-size: 1.333em;color: #fff;height: 50px;line-height: 49px;background: #000;text-align: center;margin: 0 40px 100px;}


::-webkit-scrollbar {display: none;}
::-webkit-scrollbar-thumb {background: #ff6A5b;}

@media (max-width:360px){
    .loginWrap {padding:70px 25px 52.5px;}
    .benefit_wrap .list_myevent li a {padding-left: 8%;}
    .benefit_wrap .list_myevent li a:after {right: 8%;}
    .benefit_wrap .timer_inner {height: 125px;padding-left: 8px;}
    /*.benefit_wrap .timer_inner strong {font-size:10.9vw;}*/
    .benefit_wrap .timer_inner span {width: 125px;height: 125px;}
    .benefit_wrap .cont_sale dd {padding-right: 15px;}
    .benefit_wrap .nonmember_area dl {width:85%;}
    .benefit_wrap .nonmember_area p {width:85%;}
    .benefit_wrap .timer_inner em {margin-bottom: 5px;}
    .benefit_wrap .desc_comment .saving_point {width:80px;height: 80px;}
    .benefit_wrap .desc_comment .list_saving {width: calc(100% + 10px);margin-left: -10px;margin-bottom:0;}
    .benefit_wrap .desc_comment .list_saving li p {font-size:1.2em;}
    .benefit_wrap .desc_comment .saving_point em {line-height: 85px;}
    .benefit_wrap .desc_comment .saving_point span {left: 15px;bottom: 14px;}

    .record_wrap .swiper-container.bnr-container .swiper-slide a span {padding: 3vw 0 0 40px;}
    .record_wrap .swiper-container .swiper-slide a h2 {max-width: 50%;}
    .record_wrap .realtime_chart .list_realtime li i {width: 80px;height:80px;}
    .record_wrap .realtime_chart .list_realtime li strong {margin-right: 5px;}
    .record_wrap .realtime_chart .list_realtime li .desc_real h2 {max-width: 48vw;}
    .record_wrap .recomm_wrap .album_recomm span {width:130px;height:130px;}
    .record_wrap .recomm_wrap .seemore_wrap i {width:17px;margin-right:5px;}
    .record_wrap .recomm_wrap .desc_recomm {padding: 8.5px 5px 2.5px;}

    .category_wrap .pd_list.pd_best ul li .tit {margin:1.5px 0 12px;-webkit-line-clamp: 1;height: auto;}
    .category_wrap .pd_list.pd_best ul li .price {margin-bottom:7px;font-size: 1em;}
    .category_wrap .pd_list.pd_best ul li .price .discount {display: none;}
    .category_wrap .pd_list.pd_best ul li .thum {width: 42.48%;}
    .category_wrap .pd_list.pd_best ul li .cont {width: 55%;padding-right: 0;padding-left:6px;}
    .category_wrap .pd_list.pd_best ul li .info {position: absolute;bottom:0;margin-top:5px;}
    .category_wrap .pd_list ul li .price span + span {margin-left:8px;}

    .goods_wrap.category_wrap .pd_list.pd_best ul li .thum {height:38vw;}
    .brand_wrap .topTit_brand .tit_brand {width:59%;}
    .history_inner .catalog_history li .desc_type01 {max-width: 54%;}
    .history_inner .brand_history .list_brands li .txt_brand {max-width: 45%;}
    .alarm_wrap .notice_inner a {margin-left: 9px;}
    .alarm_wrap .notice_inner span{padding: 0 18px 2px 2px;}
}
@media (min-width:768px) {
    .goods_wrap.category_wrap .pd_list.pd_best ul li {padding-left: 0;float: left;width: 45.5%;margin-right: 4%;left: 5%;}
    .goods_wrap.category_wrap .pd_list.pd_best ul li:nth-child(even) {margin-right: 0;}
    .goods_wrap.category_wrap .pd_list.pd_best ul li:before {width:fit-content;left: -8.5%;}
    .goods_wrap.category_wrap .pd_list.pd_best ul li .thum {width:46%;height:21.5vw;}
    .goods_wrap.category_wrap .pd_list.pd_best ul li .cont {width:49%;}
}
