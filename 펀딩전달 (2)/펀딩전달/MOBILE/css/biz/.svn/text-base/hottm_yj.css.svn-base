.record_wrap .culture_wrap .culture-container .desc_magazine h4 {margin-top:10px;font-size:1.167em;font-weight: 400;color:#fff;display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient:vertical;overflow: hidden;}
.record_wrap .culture_wrap .culture-container .desc_magazine span {font-size:0.917em;color:#dbdbdb;margin-top:7px;margin-right: 7px;}
/* 팬사인회 */
