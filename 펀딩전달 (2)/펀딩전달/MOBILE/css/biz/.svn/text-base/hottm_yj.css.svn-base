.record_wrap .swiper-container.bnr-container {height: 100%;}
.record_wrap .swiper-container.bnr-container .swiper-wrapper {width:100%;}
.record_wrap .swiper-container.bnr-container .swiper-slide {position: relative;width:100%;height: 100%;
  /* Center slide text vertically */
  display: -webkit-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-align: center;
  -ms-flex-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.record_wrap .swiper-container.bnr-container .swiper-slide a {position:relative;left:0;top:0;width:100%;height: 100%;min-height: 150px;}
.record_wrap .swiper-container.bnr-container .swiper-slide a h2 {max-width: 56%;font-size:2.25em;line-height: 1.185em;letter-spacing: -0.050em;word-break: keep-all;color:#fff;padding-left: 40px;overflow: hidden;display: -webkit-box;-webkit-line-clamp: 4;-webkit-box-orient: vertical;}
.record_wrap .swiper-container.bnr-container .swiper-slide a span {padding:20px 0 0 40px;max-width: 80%;font-size: 1em;color:#fff;opacity: 0.8;letter-spacing: -0.025em;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.record_wrap .swiper-container.bnr-container .swiper-slide a img {width:100%;}
.record_wrap .swiper-container.bnr-container .swiper-slide .slide_circle {z-index: 3;position: absolute;right:6%;top:26px;width:100px;height: 100px;border-radius: 100%;}
.record_wrap .swiper-container.bnr-container .swiper-slide .slide_circle .ico_badge {display:block;position:relative;top:0;width:100px;height:100px;border-radius:50%;-webkit-transform:scale(0.98);transform: scale(0.98);-webkit-transition: -webkit-transform ease-out 0.4s;transition: transform ease-out 0.4s;}
.record_wrap .swiper-container.bnr-container .swiper-slide .slide_circle .ico_badge:after {content: '';position:absolute;
	top: 0;
	right: 0%;
	padding: 0;
	z-index: -1;
	box-shadow: 0 0 0 2px rgba(255, 0, 0, 0.1);
	opacity: 0;
	-webkit-transform: scale(0.9);
	transform:scale(0.9);
	pointer-events: none;
	position: absolute;
	width: 100px;
	height: 100px;border-radius: 50%;
	-webkit-box-sizing: content-box;box-sizing: content-box;
}
.record_wrap .swiper-container.bnr-container .swiper-slide .slide_circle .ico_badge img {position: absolute;left: 50%;top: 50%;max-width: 65px;max-height: 65px;transform: translate(-50%, -50%);}
.record_wrap .swiper-container.bnr-container .swiper-slide .slide_circle .ico_badge.on {-webkit-transform:scale(1);transform: scale(1);background: #fff;}
.record_wrap .swiper-container.bnr-container .swiper-slide .slide_circle .ico_badge.on:after {-webkit-animation: sonarEffect 1.6s ease-out;animation: sonarEffect 1.6s ease-out;animation-iteration-count: infinite;-webkit-animation-iteration-count: infinite;}

@-webkit-keyframes sonarEffect {
	0% {
		opacity: 0.3;
		-webkit-box-shadow: 0 0 0 2px rgba(53,186,242, 0), 0 0 10px 10px rgba(53,186,242, 0), 0 0 0 10px rgba(53,186,242, 0);
	}
	40% {
	    opacity: 0.5;
		-webkit-box-shadow: 0 0 0 2px rgba(53,186,242, 0.1), 0 0 10px 10px #dbdbdb, 0 0 0 10px rgba(255, 255, 255, 0.5);
	}
	100% {
		-webkit-box-shadow: 0 0 0 2px rgba(53,186,242, 0.1), 0 0 10px 10px #dbdbdb, 0 0 0 10px rgba(255, 255, 255, 0.5);
		-webkit-transform: scale(1.2);
		opacity: 0;
	}
}

@keyframes sonarEffect {
	0% {
		opacity: 0.3;
		box-shadow: 0 0 0 2px rgba(53,186,242, 0), 0 0 10px 10px rgba(53,186,242, 0), 0 0 0 10px rgba(53,186,242, 0);
	}
	40% {
	    opacity: 0.5;
		box-shadow: 0 0 0 2px rgba(53,186,242, 0.1), 0 0 10px 10px #dbdbdb, 0 0 0 10px rgba(255, 255, 255, 0.5);
	}
	100% {
		box-shadow: 0 0 0 2px rgba(53,186,242, 0.1), 0 0 10px 10px #dbdbdb, 0 0 0 10px rgba(255, 255, 255, 0.5);
		transform: scale(1.2);
		opacity: 0;
	}
}

