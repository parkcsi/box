#search_detail{position:fixed; left:100%; width:calc(100% - 80px); right:0; top:0; bottom:0; box-sizing:border-box; z-index:999; overflow:hidden;}
#search_detail .search_detail_cont{position:absolute; left:0; right:0; height:100%; background:#fff; -webkit-overflow-scrolling:touch; overflow-y:auto;}
#search_detail .hd{height:59px; border-bottom:1px solid #000; padding:0 16px 0 20px;}
#search_detail .hd h2{line-height:59px;}
#search_detail .hd .btn_reset{padding-left:16px; margin-top:14px; height:30px; line-height:30px; float:right; font-size:0.700em; vertical-align:top; background:url('../../images/biz/common/icon_reset.png') 0 50% no-repeat; background-size:10px 13px;}
#search_detail .cont{position:relative;}
#search_detail .cont > ul > li a{position:relative; display:block; color:#000; font-size: 1.167em;}
#search_detail .cont > ul > li .dp1{position:relative; height:44px; padding-right:45px; line-height:44px; margin-left:20px; border-bottom:1px solid #DBDBDB;}
#search_detail .cont > ul > li .dp1:after{position:absolute; right:24px; top:50%; margin-top:-6px; content:''; background:url('../../images/biz/common/icon_arr.png') 0 0 no-repeat; background-size:24px 12px; width:12px; height:12px;}
#search_detail .cont > ul > li.active .dp1:after{background-position:100% 0;}
#search_detail .cont > ul > li.active .dp1{border-bottom:0;}
#search_detail .cont > ul > li .dp1 span{float:right; font-size:0.917em; color:#FF6956; max-width:55%; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
#search_detail .cont > ul > li .dp2{display:none; background-color:#f6f6f6;}
#search_detail .cont > ul > li .dp2 > ul > li a{padding:0 20px 0 30px; line-height:40px; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
#search_detail .cont > ul > li .dp2 .active > a:before{position:absolute; content:''; width:16px; height:16px; right:21px; top:50%; margin-top:-8px; background:url('../../images/biz/common/icon_close_depth.png') no-repeat; background-size:16px;}
#search_detail .cont > ul > li .dp3{display:none; background:#f4f4f4;}
#search_detail .cont > ul > li .dp3 > ul > li a{position:relative; padding:0 20px 0 45px; line-height:40px;}
#search_detail .cont > ul > li .dp3 > ul > li a label{padding-right:30px; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
#search_detail .cont > ul > li .dp3 > ul > li a:before{position:absolute; content:''; left:30px; top:50%; margin-top:-2px; width:5px; height:4px; background:url('../../images/biz/common/icon_depth.png') no-repeat; background-size:5px 4px;}
#search_detail .price{display:block !important; padding:20px 10px 20px 20px;}
#search_detail .price p{font-size:1.167em; margin-bottom:12px;}
#search_detail .price .form_search{display:block !important; position:relative; overflow:hidden;}
#search_detail .price .form_search .input_wrap{float:left; overflow:hidden; width:calc(100% - 60px);}
#search_detail .price .form_search input{position:relative; float:left; font-family:font_gs; width:44%; border:0; border-bottom:1px solid #000; height:33px; color:#000; font-size:1.333em; background:transparent;}
#search_detail .price .form_search span{float:left; width:12%; text-align:center; padding-top:10px; font-weight:bold; font-size:1.333em;}
#search_detail .price .form_search input:nth-child(2){float:right;}
#search_detail .price .form_search input::placeholder{color:#DBDBDB; text-align:center; font-size:0.875em; font-family:font_ns, sans-serif;}
#search_detail .price .form_search .btns{float:right; color:#000; font-size:1.167em; padding:0 8px; background-color:#fff; line-height:32px; border:1px solid #777;}
#search_detail .research{padding:35px 10px 50px 20px;}
#search_detail .research p{font-size:1.167em; margin-bottom:12px;}
#search_detail .research .form_search{position:relative; overflow:hidden;}
#search_detail .research .form_search input{float:left; width:calc(100% - 60px); border:0; border-bottom:1px solid #000; height:33px; font-weight:bold; color:#FF6A5B; font-size:1.333em; background:transparent;}
#search_detail .research .form_search .btns{float:right; color:#000; font-size:1.167em; padding:0 8px; line-height:32px; border:1px solid #777;}
#search_detail input{vertical-align:middle; font-family:font_ns, sans-serif; border:0; padding:0;}

#search_detail .form_checkbox{display:block; overflow:hidden; position:relative; vertical-align:middle; color:#000; letter-spacing:-0.05em;}
#search_detail .form_checkbox input[type="checkbox"]{height:0; opacity:0; width:0; position:absolute; left:-10px;}
#search_detail .form_checkbox .icon{position:absolute; right:0; top:50%; margin-top:-9px; width:18px; height:18px; background:url('../../images/biz/common/icon_check.png') no-repeat; background-size:36px 18px;}
#search_detail .form_checkbox input[type="checkbox"]:checked+.icon{background-position:-18px 0;}
#search_detail .form_radio{display:block; vertical-align:top; cursor:pointer; position:relative;}
#search_detail .form_radio input[type="radio"]{position:absolute; left:-50px; top:-50px; opacity:0; width:0px; height:0px; border:0; background-color:transparent}
#search_detail .form_radio span{color:#000;}
#search_detail .form_radio span:before{position:absolute; right:0; top:50%; margin-top:-9px; content:''; background:url('../../images/biz/common/icon_radio.png') no-repeat; background-size:36px 18px; width:18px; height:18px; display:inline-block; vertical-align:top;}
#search_detail .form_radio input[type="radio"]:checked + span:before{background-position:-18px 0;}

#wrap .search_dim{background-color:#000; position:fixed; left:0; top:0; width:100%; height:100%; z-index:998; opacity:0; overflow: hidden;}

