.font_t{font-family:font_gs, sans-serif;}
.content_new th, td, input, select, textarea, button{font-family:font_ns, sans-serif;}
.content_new button{outline:none;}

.content_new{font-family:'font_ns', AppleGothic, sans-serif; color:#000;}
.content_new #content{margin-top:61px; padding:0 0 180px 0;}
.content_new.main #content{margin-top:170px;}
.content_new.main #header .top_menu{display:block;}
.content_new.main #header .main_wrap{display:block;}
.content_new.main #header .sub_wrap{display:none;}
