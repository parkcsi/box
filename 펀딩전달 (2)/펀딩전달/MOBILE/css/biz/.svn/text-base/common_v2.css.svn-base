.content_new.main #header.active + #content > .swiper-container{margin-top:-10px;}

body.fixed{overflow-y:hidden;}
body.bg_record.fixed{overflow-y:hidden;}

/* header */
#header{position:fixed; top:0; left:0; width:100%; border-bottom:1px solid #DBDBDB; background-color:#fff; z-index:98;}
#header .dim{background-color:#000; position:fixed; left:0; top:0; width:100%; height:100%; z-index:8; opacity:0}
#header .inner{position:relative; height:60px; box-sizing:border-box;}
#header .inner .btn_category{position:absolute; right:15px; top:13px; width:34px; height:34px; text-indent:-9999px; font-size:0; background:url('../../images/biz/common/icon_sidemenu.png') 50% 50% no-repeat; background-size:24px;}
#header .inner .btn_search{position:absolute; right:53px; top:13px; width:34px; height:34px; text-indent:-9999px; font-size:0; background:url('../../images/biz/common/icon_search.png') 50% 50% no-repeat; background-size:24px;}
#header .inner .btn_back{position:absolute; left:15px; top:13px; width:34px; height:34px; overflow:hidden; text-indent:-9999px; font-size:0; background: url('../../images/biz/common/icon_back.png') 50% 50% no-repeat; background-size:27px;}
#header .inner h2{font-size:1.667em; padding:0 80px; overflow:hidden; text-align:center; line-height:60px; box-sizing:border-box; white-space:nowrap; text-overflow:ellipsis;}
#header .inner .main_wrap{display:none; padding-right:95px;}

#header{position:fixed; top:0; left:0; border-bottom:1px solid #DBDBDB; background-color:#fff; z-index:98;}
#header .dim{background-color:#000; position:fixed; left:0; top:0; width:100%; height:100%; z-index:8; opacity:0}
.main #header .inner{position:relative; padding:37px 0 0 40px; margin-bottom:27px; height:83px; box-sizing:border-box;}
.main #header .btn_home{position:absolute; text-indent:-9999px; content:''; padding:5px; bottom:-13px; left:6px; right:0; width:19px; height:19px; background:url('../../images/biz/common/icon_simbol.png') 50% 50% no-repeat; background-size:19px; z-index:2;}
.main #header .inner:after{position:absolute; content:''; bottom:0; left:40px; right:0; height:3px; background:#000;}
.main #header .inner input{display:inline-block; background:transparent; color:#000; font-size:1.333em; line-height:24px; text-indent:10px; width:100%; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
.main #header .inner input::placeholder{color:#ABABAB; font-size:1em; font-family:font_ns, sans-serif;}
.main #header .inner input{vertical-align:middle; font-family:font_ns, sans-serif; border:0; padding:0;}
.main #header .inner .btn_category{top:32px;}
.main #header .inner .btn_search{top:32px;}
#header .btn_home{display:none;}

#header .top_menu{display:none; position:relative; height:60px;}
#header .top_menu ul{padding-top:2px; padding-left:30px;}
#header.active .top_menu ul{padding-top:12px;}
#header .top_menu ul li{position:relative; padding-right:8px; padding-left:10px;}
#header .top_menu ul li a{color:#777; font-weight:300; font-size:1.667em; padding:5px 0;}
#header .top_menu ul li{float:left;}
#header .top_menu ul li.active a{font-weight:bold; letter-spacing:-0.05em; color:#DA2128;}
#header .top_menu ul li.active a:before{position:absolute; content:''; top:0; width:6px; height:6px; background:#DA2128; border-radius:3px;}
#header .top_menu ul li.active a .this {position: absolute;right: -5px;top: 3px;width: 18px;height: 15px;background: url(../../images/biz/sub/img_wink.png) no-repeat;background-size: contain;}

@media screen and (max-width: 360px){
    #header .top_menu ul{padding-left:34px;}
    #header .top_menu ul li{position:relative; padding-right:6px; padding-left:6px;}
    #header .top_menu ul li a{font-size:1.500em;}
}

#header .agit_menu{padding-bottom:10px;}
#header .agit_menu ul{overflow:hidden; width:100%;}
#header .agit_menu ul li{position:relative; float:left; width:20%; text-align:center;}
#header .agit_menu ul li i{position:relative; display:block; margin:0 auto; margin-bottom:4px; width:42px; height:42px; border-radius:22px; overflow:hidden; border:1px solid #DBDBDB;}
#header .agit_menu ul li a{color:#777; letter-spacing:-0.04em;}
#header .agit_menu ul li .icon01:before{position:absolute; content:''; width:16px; height:25px; left:50%; margin-left:-8px; top:50%; margin-top:-13px; background:url('../../images/biz/sub/icon_agit01.png') no-repeat; background-size:32px 25px;}
#header .agit_menu ul li .icon02:before{
    position: absolute;
    content: '';
    width: 21.5px;
    height: 24px;
    left: 50%;
    margin-left: -9.5px;
    top: 51.5%;
    margin-top: -13px;
    background: url(http://image.kyobobook.co.kr/newimages/giftshop_new/resources/img/common/icon_agit02.png) no-repeat;
    background-size: 43px 24px;
}
#header .agit_menu ul li .icon03:before{position:absolute; content:''; width:27px; height:30px; left:50%; margin-left:-13px; top:50%; margin-top:-15px; background:url('../../images/biz/sub/icon_agit03.png') no-repeat; background-size:54px 30px;}
#header .agit_menu ul li .icon04:before{position:absolute; content:''; width:21px; height:20px; left:50%; margin-left:-10px; top:50%; margin-top:-9px; background:url('../../images/biz/sub/icon_agit04.png') no-repeat; background-size:42px 20px;}
#header .agit_menu ul li .icon05:before{position:absolute; content:''; width:28px; height:25px; left:50%; margin-left:-14px; top:50%; margin-top:-13px; background:url('../../images/biz/sub/icon_agit05.png') no-repeat; background-size:56px 25px;}
#header .agit_menu ul li.active a{color:#000;}
#header .agit_menu ul li.active i{border-color:#2DC8C4;}
#header .agit_menu ul li.active .icon01:before{background-position:-16px 0;}
#header .agit_menu ul li.active .icon02:before{background-position:-21.5px 0;}
#header .agit_menu ul li.active .icon03:before{background-position:-27px 0;}
#header .agit_menu ul li.active .icon04:before{background-position:-21px 0;}
#header .agit_menu ul li.active .icon05:before{background-position:-28px 0;}

/* footer */
#footer{width:100%; margin:0; padding:0 0 110px 40px; text-align:left; border-top:0; font-size:1em; color:#000; background-color:#fff; box-sizing:border-box;padding-bottom: constant(safe-area-inset-bottom);padding-bottom: env(safe-area-inset-bottom);}
#footer .inner{border-top:1px solid #808080; padding-top:25px; padding-right:20px;padding-bottom:110px;}
#footer .inner p{font-weight:300; font-size:1.667em;}
#footer .inner p em{font-weight:300; font-size:1.050em;}
#footer .inner .util_menu{margin-top:14px;}
#footer .inner .util_menu ul{overflow:hidden;}
#footer .inner .util_menu ul li{float:left;}
#footer .inner .util_menu ul li + li{margin-left:16px;}
#footer .inner .util_menu ul li a{display:block; color:#000; font-size:0.833em;}
#footer .inner .btn_cont{position:relative; padding-right:20px; display:inline-block; font-weight:bold; font-size:1.333em; color:#000;}
#footer .inner .btn_cont:after{position:absolute; content:''; top:50%; margin-top:-5px; width:17px; height:10px; right:0; background:url('../../images/biz/common/icon_footer_arrow.png') 50% 50% no-repeat; background-size:17px 10px;}
#footer .inner .btn_cont.open:after{position:absolute; content:''; top:50%; margin-top:-5px; width:17px; height:10px; right:0; background:url('../../images/biz/common/icon_footer_arrow.png') 50% 50% no-repeat;transform:rotate(180deg);background-size:17px 10px;}
#footer .inner .cont{position:relative; margin:16px 0 25px;}
#footer .inner .cont > img{height:36px;}
#footer .inner .sns_wrap{float:right; overflow:hidden; margin-top:9px;}
#footer .inner .sns_wrap li{float:left; width:18px; height:17px; background:url('../../images/biz/common/icon_sns.png') no-repeat; background-size:90px 17px;}
#footer .inner .sns_wrap li a{display:block; height:100%; text-indent:-9999px;}
#footer .inner .sns_wrap li.fb{background-position:-18px 0;}
#footer .inner .sns_wrap li.tt{background-position:-36px 0;}
#footer .inner .sns_wrap li.bg{background-position:-54px 0;}
#footer .inner .sns_wrap li.yt{background-position:-72px 0;}
#footer .inner .sns_wrap li + li{margin-left:14px;}
#footer .txt{display:none; color:#707070; margin-top:20px;}
#footer .txt.on{display:block;}
#footer .txt span{display:block; font-size:0.917em;}
#footer .txt span + span{margin-top:4px;}
#footer .txt .copyright{color:#999999; margin-top:20px; letter-spacing:0.1em;}

@media screen and (max-width: 360px){
    #footer .inner .cont > img{height:30px;}
    #footer .inner .sns_wrap{margin-top:8px;}
    #footer .inner .sns_wrap li + li{margin-left:8px;}
}

#app_bar{position:fixed; height:55px; bottom:0; left:0; right:0; border-top:1px solid #DBDBDB; background-color:#fff; z-index:99;padding-bottom: constant(safe-area-inset-bottom);padding-bottom: env(safe-area-inset-bottom);}
#app_bar .btn_today{position:relative; display:block; width:36px; height:36px; text-indent:-9999px; background:url('../../images/biz/common/icon_ball.png') 0 0 no-repeat; position:relative; margin:10px auto 0; background-size:100%;}
#app_bar .btn_home{position:absolute; left:10px; top:10px; padding:10px; width:57px; height:16px; overflow:hidden; text-indent:-9999px; font-size:0; background: url('../../images/biz/common/icon_home.png') 50% 50% no-repeat; background-size:57px 16px;}
#app_bar .btn_cart{position:absolute; right:75px; top:11px; width:34px; height:34px; text-indent:-9999px; background: url('../../images/biz/common/icon_cart.png') 50% 50% no-repeat; background-size:27px;}
#app_bar .btn_cart em{display:block; position:absolute; top:-2px; right:-12px; padding:4px 6px 2px; min-width:26px; max-width:32px; box-sizing:border-box; overflow:hidden; text-align:center; text-indent:initial; font-size:0.8em; font-weight:bold; color:#fff; background:#da2128; border-radius:10px; letter-spacing:-0.05em;}
#app_bar .btn_history{position:absolute; right:18px; top:11px; width:34px; height:34px; overflow:hidden; text-indent:-9999px; font-size:0; background: url('../../images/biz/common/icon_history.png') 50% 50% no-repeat; background-size:27px;}
#app_bar .btn_history.on{padding:3px 5px 5px 3px; box-sizing:border-box; background:none; text-indent:initial;}
#app_bar .btn_history.on:before{position:absolute; content:''; top:0; left:0; width:34px; height:34px; background: url('../../images/biz/common/icon_history_on.png') 50% 50% no-repeat; background-size:30px;}
#app_bar .btn_history img{width:100%; height:100%;}

/* top·bottom 버튼 */
.foot_btn.btn_top{bottom:120px;
    -webkit-border-radius:100%;border-radius: 100%;box-sizing: border-box;overflow:hidden;
    background:#fff url('../../images/biz/common/icon_top.png') no-repeat center 0;background-size:contain;
    -webkit-box-shadow:0 0 10px 0 rgba(0,0,0,0.3); -moz-box-shadow:0 0 10px 0 rgba(0,0,0,0.3); box-shadow:0 0 10px 0 rgba(0,0,0,0.3);
    -webkit-appearance: none;
    -webkit-tap-highlight-color: transparent;margin-bottom: constant(safe-area-inset-bottom);margin-bottom: env(safe-area-inset-bottom);}
.foot_btn.btn_down{
    border-top-right-radius: 100%;border-bottom-right-radius: 100%;border-bottom-left-radius: 100%;border-top-left-radius: 100%;box-sizing: border-box;overflow:hidden;
    background:#fff url('../../images/biz/common/icon_down.png') no-repeat center 0;background-size:contain;
    -webkit-box-shadow:0 0 10px 0 rgba(0,0,0,0.3); -moz-box-shadow:0 0 10px 0 rgba(0,0,0,0.3); box-shadow:0 0 10px 0 rgba(0,0,0,0.3);
    -webkit-appearance: none;
    -webkit-tap-highlight-color: transparent;margin-bottom: constant(safe-area-inset-bottom);margin-bottom: env(safe-area-inset-bottom);}

.order_wrap{position:fixed; height:55px; bottom:0; left:0; right:0; border-top:1px solid #DBDBDB; background-color:#fff; overflow:hidden; z-index:99;}
.order_wrap .btn_wrap{position:relative; float:left; width:34%; height:100%;}
.order_wrap .btn_buy{display:block; float:left; width:66%; text-align:center; color:#fff; background:rgba(0,0,0,1);}
.order_wrap .btn_buy span{line-height:55px; font-size:1.750em; font-weight:bold; letter-spacing:0.1em;}
.order_wrap .btn_share{position:absolute; left:14%; bottom:10px; width:34px; height:34px; text-indent:-9999px; background: url('../../images/biz/common/icon_share.png') 50% 50% no-repeat; background-size:24px;}
.order_wrap .btn_heart{position:absolute; right:23%; bottom:17px; width:24px; height:19px; text-indent:-9999px; background: url('../../images/biz/common/icon_heart.png') no-repeat; background-size:48px 19px;}
.order_wrap .btn_heart em{display:block; position:absolute; top:-10px; right:-21px; padding:0 5px; height:20px; min-width:20px; max-width:22px; overflow:hidden; text-align:center; text-indent:initial; font-size:1.167em; font-weight:bold; color:#fff; background:#da2128; border-radius:10px; line-height:20px; letter-spacing:-0.05em;}
.order_wrap .btn_heart.on{background-position:-24px 0;}

/* side menu */
#sidemenu_wrapper{position:fixed; left:100%; top:0; bottom:0; width:100%; background:#fff;box-sizing:border-box; z-index:99; overflow:auto;}
#sidemenu_wrapper > .swiper-container{width:100%; height:100%; box-sizing:border-box;}
#sidemenu_wrapper > .swiper-container .swiper-slide{height:auto !important}
#sidemenu_wrapper .sidemenu_cont{position:relative; height:100%; padding-bottom:40px;}

#sidemenu_wrapper .hd{background-color:#333; min-height:270px; position:relative; width:100%; z-index:3;}
#sidemenu_wrapper .hd .cont{padding:70px 40px 0;}
#sidemenu_wrapper .hd .cont > p{color:#fff; font-size:2.250em; font-weight:bold; letter-spacing:-0.02em;}
#sidemenu_wrapper .hd .cont > p span{display:inline-block; max-width:52%; white-space:nowrap; overflow:hidden; text-overflow:ellipsis; vertical-align:top;}
#sidemenu_wrapper .hd .cont > p strong{display:inline-block; text-decoration:underline;}
#sidemenu_wrapper .hd ul{overflow:hidden; margin-top:18px;}
#sidemenu_wrapper .hd ul li{float:left; width:25%; text-align:center;}
#sidemenu_wrapper .hd ul li a{display:block; color:#fff; font-size:1em; padding:10px 0 0;}
#sidemenu_wrapper .hd ul li i{position:relative; display:inline-block; width:24px; height:24px; background:url('../../images/biz/common/icon_menu.png') no-repeat 0 0; background-size:144px 24px;}
#sidemenu_wrapper .hd ul li .ico01{background-position:0 0;}
#sidemenu_wrapper .hd ul li .ico02{background-position:-24px 0;}
#sidemenu_wrapper .hd ul li .ico03{background-position:-48px 0;}
#sidemenu_wrapper .hd ul li .ico04{background-position:-73px 0;}
#sidemenu_wrapper .hd ul li .ico05{background-position:-97px 0;width:23px;}
#sidemenu_wrapper .hd ul li .ico06{background-position:-120px 0;}
#sidemenu_wrapper .hd ul li i em{display:block; position:absolute; top:-10px; right:-10px; width:20px; font-size:1em; font-weight:bold; color:#DA2128; background:#fff; border-radius:50%; line-height:20px; letter-spacing:-0.05em;}
#sidemenu_wrapper .hd ul li span{position:relative; display:block; margin-top:8px;}
#sidemenu_wrapper .hd ul li span em{content:''; position:absolute; display:inline-block; margin-left:2px;width:4px; height:4px; border-radius:2px; background:#ff6a5b;}

#sidemenu_wrapper .hd.grade01{background-color:#69B23A;}
#sidemenu_wrapper .hd.grade02{background-color:#939393;}
#sidemenu_wrapper .hd.grade03{background-color:#E1B641;}
#sidemenu_wrapper .hd.grade04{background-color:#543FB2;}
#sidemenu_wrapper .hd.grade99{background-color:#131F43;}
#sidemenu_wrapper .hd.no_login{background-color:#333;}

#sidemenu_wrapper .store{position:absolute; left:0; right:0; bottom:0; padding:16px 30px 17px; background:rgba(0,0,0,0.12);}
#sidemenu_wrapper .store dl{overflow:hidden; color:#fff;}
#sidemenu_wrapper .store dt,
#sidemenu_wrapper .store dd{line-height:24px;}
#sidemenu_wrapper .store dt{float:left; font-size:1.167em;}
#sidemenu_wrapper .store dt a{color: #fff;}
#sidemenu_wrapper .store dd{margin-left:140px; font-size:1.333em; background:url('../../images/biz/common/icon_location.png') no-repeat; background-size:24px;}
#sidemenu_wrapper .store dd a{display:inline-block; vertical-align:top; max-width:100%; color:#fff; padding-left:26px; white-space:nowrap; overflow:hidden; text-overflow:ellipsis; box-sizing:border-box;}

#sidemenu_wrapper .btn_wrap{position:absolute; top:20px; right:14px;}
#sidemenu_wrapper .btn_wrap button{padding:8px; width:24px; height:24px; background:url('../../images/biz/common/icon_gnb.png') no-repeat; background-size:120px 48px; cursor:pointer;}
#sidemenu_wrapper .btn_wrap .btn_alarm{position:absolute; overflow:visible; right:48px; top:0; background-position:-48px 0;}
#sidemenu_wrapper .btn_wrap .btn_alarm em{display:block; position:absolute; top:-8px; right:-8px; width:20px; font-size:12px; font-weight:bold; color:#DA2128; background:#fff; border-radius:50%; line-height:20px; letter-spacing:-0.05em;}
#sidemenu_wrapper .btn_wrap .btn_sidemenu_close{position:absolute; right:0; top:0; background-position:-24px 0;}

@media screen and (max-width: 360px){
    #sidemenu_wrapper .hd .cont{padding-left:20px; padding-right:20px;}
    #sidemenu_wrapper .hd .cont > p span{max-width:48%;}
}

/* gnb */
#gnb{padding-bottom:150px;/*position:absolute; left:0; top:100px; width:100%; bottom:0*/}

#gnb .banner{position:relative; padding:20px 30px 5px;}
#gnb .banner .swiper-container .swiper-wrapper{padding-top:10px;}
#gnb .banner .swiper-container .swiper-slide a{position:relative; display:block; vertical-align:top; height:24.52vw; color:#000;}
#gnb .banner .swiper-container .swiper-slide a .text{position:absolute; display:table; width:52%; height:100%; padding:12px 0 0 12px; font-size:1.333em; box-sizing:border-box; z-index:3;}
#gnb .banner .swiper-container .swiper-slide a .text span{display:table-cell; vertical-align:middle;}
#gnb .banner .swiper-container .swiper-slide a .text span em{display:-webkit-box; overflow:hidden; text-overflow:ellipsis; -webkit-line-clamp:2; -webkit-box-orient:vertical;}
#gnb .banner .swiper-container .swiper-slide a:after{position:absolute; content:''; left:0; top:0; right:0; width:100%; height:100%; z-index:2; background:url('../../images/biz/common/bg_banner.png') no-repeat; background-size:100% 100%;}
#gnb .banner .swiper-container .swiper-slide a img{min-width:59%; height:100%; float:right;}
#gnb .banner .swiper-container .swiper-pagination{top:0px; text-align:left; width:auto; bottom:auto;}
#gnb .banner .swiper-container .swiper-pagination-bullet{margin:0 2px; width:4px; height:4px; vertical-align:top; background:#eee; opacity:1;}
#gnb .banner .swiper-container .swiper-pagination-bullet-active {background:#DA2128;}

#gnb .category{position:relative; padding:0 30px;}
#gnb .category ul{overflow:hidden;}
#gnb .category ul li{float:left; width:25%; text-align:center; margin-top:26px;}
#gnb .category ul li a{position:relative; display:block; color:#000;}
#gnb .category ul li a i{content:''; display:inline-block; width:69px; height:76px; background:url('../../images/biz/common/icon_category.png') no-repeat; background-size:552px 152px;}
#gnb .category ul li a .ico01{background-position:0 0;}
#gnb .category ul li a .ico02{background-position:-69px 0;}
#gnb .category ul li a .ico03{background-position:-138px 0;}
#gnb .category ul li a .ico04{background-position:-207px 0;}
#gnb .category ul li a .ico05{background-position:-276px 0;}
#gnb .category ul li a .ico06{background-position:-345px 0;}
#gnb .category ul li a .ico07{background-position:-414px 0;}
#gnb .category ul li a .ico08{background-position:-483px 0;}
#gnb .category ul li a .ico09{background-position:0 -76px;}
#gnb .category ul li a .ico10{background-position:-69px -76px;}
#gnb .category ul li a .ico11{background-position:-138px -76px;}
#gnb .category ul li a .ico12{background-position:-207px -76px;}
#gnb .category ul li a .ico13{background-position:-276px -76px;}
#gnb .category ul li a .ico14{background-position:-345px -76px;}
