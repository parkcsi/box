/* 이벤트 음반 전체보기 레이어 */
#layer_banner_r{position:fixed; left:100%; top:0; bottom:0; width:100%; background:#fff; box-sizing:border-box; z-index:9999; overflow:auto;}
#layer_banner_r .layer_wrap{position:relative; height:100%;width: 100%;overflow: hidden;overflow-y: auto;}
#layer_banner_r .layer_banner_top{position:absolute; top:0; right:0; left:0; border-bottom:1px solid #DBDBDB; background-color:#fff; z-index:99;}
#layer_banner_r .layer_banner_top .btn_close{position:absolute; right:15px; top:13px; width:34px; height:34px; overflow:hidden; text-indent:-9999px; font-size:0; background: url('../../images/biz/common/btn_close_layer.png') 50% 50% no-repeat; background-size:15px;}
#layer_banner_r .layer_banner_top h2{font-size:1.667em; letter-spacing:0.1em; text-align:center; line-height:60px; box-sizing:border-box; white-space:nowrap; text-overflow:ellipsis;}
#layer_banner_r .swiper-container{width:100%; height:100%; box-sizing:border-box;}
#layer_banner_r .swiper-container .swiper-slide{height:auto !important}
#layer_banner_r .layer_cont{padding-top:61px;}
#layer_banner_r .layer_cont ul li{position:relative;}
#layer_banner_r .layer_cont ul li + li{border-top:1px solid #707070;}
#layer_banner_r .layer_cont ul li a{display:block;}
#layer_banner_r .layer_cont ul li a img{width:100%;}
#layer_banner_r .layer_cont ul li .percent{position:absolute; top:0; text-align:center; padding-top:40px; font-weight:bold; right:20px; color:#fff; font-size:1.333em; width:60px; height:64px; box-sizing:border-box; background:#da2128;}
#layer_banner_r .layer_cont ul li.type01 .info{position: absolute;left: 0;bottom: 0;width: 100%;padding-left: 40px;padding-bottom: 15px;margin-right: 10px;letter-spacing: -0.08em;background: #fff;}
#layer_banner_r .layer_cont ul li.type01 .tit{margin:15px 0 10px; font-size:2.250em; line-height:32px; font-weight:bold; color:#000; overflow:hidden; text-overflow:ellipsis; display:-webkit-box; -webkit-line-clamp:2; -webkit-box-orient:vertical;}
#layer_banner_r .layer_cont ul li.type01 .txt{font-size:1em; color:#000; white-space:nowrap; text-overflow:ellipsis; overflow:hidden;}
#layer_banner_r .layer_cont ul li.type03 {padding:15px 0 10px;}
#layer_banner_r .layer_cont ul li.type03 h2 {max-width: 56%;font-size:2.25em;line-height: 1.185em;letter-spacing: -0.050em;word-break: keep-all;padding-left: 40px;overflow: hidden;display: -webkit-box;-webkit-line-clamp: 4;-webkit-box-orient: vertical;}
#layer_banner_r .layer_cont ul li.type03 a>span {padding: 20px 0 0 40px;max-width: 80%;font-size: 1em;opacity: 0.8;letter-spacing: -0.025em;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
#layer_banner_r .layer_cont ul li.type03 .slide_circle {z-index: 3;position: absolute;right:6%;top:26px;width:100px;height: 100px;}
#layer_banner_r .layer_cont ul li.type03 .slide_circle .ico_badge {display:block;position: relative;width:100px;height:100px;border-radius: 100%;overflow: hidden;border: 1px solid #dbdbdb;}


/* 페이지 */
.pagination_v2{text-align:center;}
.pagination_v2 a{display:inline-block; color:#000; width:24px; height:24px; padding-top:5px; border-radius:12px; font-family:font_gs; font-weight:normal; font-size:1.167em; text-align:center; margin:0 2px; box-sizing:border-box;}
.pagination_v2 .active{display:inline-block; color:#fff; background:#000; font-weight:bold;}
.pagination_v2 .arr{display:inline-block; padding-top:3px; width:18px;}
.pagination_v2 .arr:after{content:''; display:inline-block; width:10px; height:17px; background:url('../../images/biz/sub/icon_page_arr.png') no-repeat; background-size:40px 17px;}
.pagination_v2 .arr.prev{margin-right:10px;}
.pagination_v2 .arr.next{margin-left:10px;}
.pagination_v2 .arr.prev:after{background-position:-10px 0;}
.pagination_v2 .arr.first:after{background-position:0 0;}
.pagination_v2 .arr.next:after{background-position:-20px 0;}
.pagination_v2 .arr.last:after{background-position:-30px 0;}

@media screen and (max-width: 360px){
    .pagination_v2 a{font-size:1.083em; width:22px; height:22px; padding-top:5px; border-radius:11px; margin:0;}
    .pagination_v2 .arr.prev{margin-right:5px;}
    .pagination_v2 .arr.next{margin-left:5px;}
}

/* content */
